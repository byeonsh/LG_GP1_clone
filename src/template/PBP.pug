    script(src="../components/PD0004/PD0004.js" type="module")
    script(src="../components/CM0004/CM0004.js" type="module")
    script(src="../components/CM0007/CM0007.js" type="module")

block header
    include ../components/CM0001/CM0001-inc.pug
    +CM0001('PBP')

block content
    include ../components/PD0003/PD0003-inc.pug
    +PD0003('')

    include ../components/ST0053/ST0053-inc.pug
    +ST0053('AWARD')

    include ../components/CM0004/CM0004-inc.pug
    +CM0004('PBP')

    include ../components/PD0004/PD0004-inc.pug
    +PD0004('BV2')

    include ../components/CM0010/CM0010-inc.pug
    +CM0010('PBP')

    include ../components/ST0023/ST0023-inc.pug
    +ST0023('PBP')
    
    include ../components/PD0006/PD0006-inc.pug
    +PD0006('LENGTH4')

    include ../components/CM0007/CM0007-inc.pug
    +CM0007()

block footer
    include ../components/CM0002/CM0002-inc.pug
    +CM0002()

block modal 
