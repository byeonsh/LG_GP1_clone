    mixin accordion(items)
        .accordion(role='tablist')
            each item, i in itemsIT
                .accordion__item
                    .accordion__title(role='tab', aria-expanded=i === 0 ? 'true' : 'false')
                        strong.font-m-normal-20.font-w-normal-32 #{item.prdTypes} 

                    .accordion__content(role='tabpanel')
                        if item.prdname
                            .c-warranty__text
                                dl 
                                    dt.font-m-semibold-14.font-w-normal-14 Prodotto
                                    dd.font-m-normal-14.font-w-normal-24 #{item.prdname}
                                dl 
                                    dt.font-m-semibold-14.font-w-normal-14 Periodo di garanzia
                                    dd.font-m-normal-14.font-w-normal-16 #{item.perioddue}
                                if item.conditionstatus
                                    dl 
                                        dt.font-m-semibold-14.font-w-normal-14 Condizioni di garanzia aggiuntive
                                        dd.font-m-normal-14.font-w-normal-16 #{item.conditionstatus}
                                            if item.conditionbold3
                                                span.cs-bold #{item.conditionbold3}
                                                | #{item.conditionstatus1a}
                                else 
                                    block
                                if item.conditionboldtype
                                    dl 
                                        dt.font-m-semibold-14.font-w-normal-14 Condizioni di garanzia aggiuntive
                                        dd.font-m-normal-14.font-w-normal-16
                                            if item.conditionboldtype
                                                span.cs-bold #{item.conditionboldtype}
                                                br
                                                | #{item.conditionstatus2atype}
                                                span.cs-bold #{item.conditionbold2type}
                                                | #{item.conditionstatus2btype}
                                                if item.conditionstatus2c
                                                    br
                                                    | #{item.conditionstatus2c}
                                else 
                                    block
                        if item.prdname2
                            .c-warranty__text
                                dl 
                                    dt.font-m-semibold-14.font-w-normal-14.c-hidden-pc Prodotto
                                    dd.font-m-normal-14.font-w-normal-24 #{item.prdname2}
                                dl 
                                    dt.font-m-semibold-14.font-w-normal-14.c-hidden-pc Periodo di garanzia
                                    dd.font-m-normal-14.font-w-normal-16 #{item.perioddue2}
                                if item.conditionstatus2
                                    dl 
                                        dt.font-m-semibold-14.font-w-normal-14.c-hidden-pc Condizioni di garanzia aggiuntive
                                        dd.font-m-normal-14.font-w-normal-16 #{item.conditionstatus2}
                                            if item.conditionbold
                                                br
                                                span.cs-bold #{item.conditionbold}
                                                br
                                                | #{item.conditionstatus2a}
                                                span.cs-bold #{item.conditionbold2}
                                                | #{item.conditionstatus2b}
                                                if item.conditionstatus2c
                                                    br
                                                    | #{item.conditionstatus2c}
                                else 
                                    dl 
                                        dt.font-m-semibold-14.font-w-normal-14.c-hidden-pc Condizioni di garanzia aggiuntive
                                        dd.font-m-normal-14.font-w-normal-16
                                            if item.conditionbold
                                                span.cs-bold #{item.conditionbold}
                                                br
                                                | #{item.conditionstatus2a}
                                                span.cs-bold #{item.conditionbold2}
                                                | #{item.conditionstatus2b}
                                                if item.conditionstatus2c
                                                    br
                                                    | #{item.conditionstatus2c}
                        if item.prdnamespecial 
                            .c-warranty__text
                                dl 
                                    dt.font-m-semibold-14.font-w-normal-14.c-hidden-pc Prodotto
                                    dd.font-m-normal-14.font-w-normal-24 #{item.prdnamespecial}
                                dl 
                                    dt.font-m-semibold-14.font-w-normal-14.c-hidden-pc Periodo di garanzia
                                    dd.font-m-normal-14.font-w-normal-16 #{item.periodduespecial}
                                if item.conditionstatusspecial
                                    dl 
                                        dt.font-m-semibold-14.font-w-normal-14.c-hidden-pc Condizioni di garanzia aggiuntive
                                        dd.font-m-normal-14.font-w-normal-16 #{item.conditionstatusspecial}
                                            if item.conditionboldspecial
                                                span.cs-bold #{item.conditionboldspecial}
                                                | #{item.conditionstatusspecial1}        
                        if item.prdnamespecial2
                            .c-warranty__text
                                dl 
                                    dt.font-m-semibold-14.font-w-normal-14.c-hidden-pc Prodotto
                                    dd.font-m-normal-14.font-w-normal-24 #{item.prdnamespecial2}
                                dl 
                                    dt.font-m-semibold-14.font-w-normal-14.c-hidden-pc Periodo di garanzia
                                    dd.font-m-normal-14.font-w-normal-16 #{item.periodduespecial2}
                                if item.conditionstatusspecial2
