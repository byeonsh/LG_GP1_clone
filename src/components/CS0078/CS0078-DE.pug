    mixin accordion(items)
        .accordion(role='tablist')
            each item, i in items
                .accordion__item
                    .accordion__title(role='tab', aria-expanded=i === 0 ? 'true' : 'false')
                        strong.font-m-normal-20.font-w-normal-32 #{item.prdTypes}

                    .accordion__content(role='tabpanel')
                        .c-warranty__text
                            p.font-m-normal-14.font-w-normal-14 #{item.warranty}
                            ul.font-m-normal-14.font-w-normal-24 
                                li 
                                    span Labour :
                                    | #{item.labour}
                                li 
                                    span Parts : 
                                    | #{item.parts}
                            if beforeNote
                                .c-beforeNote.font-m-normal-14.font-w-normal-16.c-hidden-pc 
                                    | #{beforeNote}
                                    a(href="#").c-hidden-pc #{item.beforeNoteLink_mo}
                                    a(href="#").c-hidden-mo #{item.beforeNoteLink_pc}
                                    | #{item.beforeNote2}
                                .button.c-cta.c-hidden-pc
                                    +linkButton({name:'Learn more', icon:true, type:'text'})
                            if item.note
                                .font-m-normal-14.font-w-normal-16.aaaa 
                                    if item.beforeNoteText 
                                        .c-beforeNote.font-m-normal-14.font-w-normal-16
                                            | #{item.beforeNoteText}
                                            a(href="#").c-hidden-pc #{item.beforeNoteLink_mo}
                                            a(href="#").c-hidden-mo #{item.beforeNoteLink_pc}
                                            | #{item.beforeNote2} 
                                    else
                                    if item.moreBtn
                                        .button.c-cta
                                            +textButton({name:'Learn more', wSize:'small', mSize:'small', icon:true, type:'text'})
                                    else 
                                        block
                                    .c-note-flag.font-m-normal-14.font-w-normal-14 Note
                            if !item.note
                                if item.beforeNoteText
                                    .c-no-note.font-m-normal-14.font-w-normal-16.ccc 
                                        .c-beforeNote.font-m-normal-14.font-w-normal-16
                                            .c-beforeNote__text
                                                | #{item.beforeNoteText}
                                                //- a(href="#").c-hidden-pc #{item.beforeNoteLink_mo}
                                                //- a(href="#").c-hidden-mo #{item.beforeNoteLink_pc}
                                                //- | #{item.beforeNote2}
                                        if item.moreBtn
                                            .button.c-cta
                                                +textButton({name:'Learn more', wSize:'small', mSize:'small',icon:true, type:'text'})
                                else 
                                    if item.moreBtn
                                        .button.c-cta
                                            +textButton({name:'Learn more', wSize:'small', mSize:'small',icon:true, type:'text'})
                            div.c-noteText.font-m-normal-14.font-w-normal-16 #{item.noteText}

