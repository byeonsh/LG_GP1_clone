    mixin accordionContents(faq, useImg, controlsId, beforeNote, note, note2)
        .c-accordion__head
            // block - text contents
            .c-accordion__head-text.c-text-contents
                span.c-accordion__initial.font-w-normal-20.font-m-normal-18.font-normal(aria-hidden="true") Q.
                .text.c-text-contents__headline
                    .cmp-text.font-w-normal-20.font-m-normal-20(data-cmp-data-layer="")
                        strong.font-m-normal-20.font-w-normal-32 #{faq.prdTypes}
                       
                        .c-warranty__text
                            p.font-m-normal-14.font-w-normal-14 #{faq.warranty}  
                            ul.font-m-normal-14.font-w-normal-24
                                li
                                    span Labour :
                                    | #{faq.labour}
                                li
                                    span Parts :
                                    | #{faq.parts}
                            if beforeNote
                                .c-beforeNote.font-m-normal-14.font-w-normal-16.c-hidden-pc
                                    | #{beforeNote}
                                    a(href="#").c-hidden-pc #{faq.beforeNoteLink_mo}
                                    a(href="#").c-hidden-mo #{faq.beforeNoteLink_pc}
                                    | #{faq.beforeNote2}
                                if faq.url
                                    .button.c-cta.c-hidden-pc
                                        +linkButton({name:'Learn more', icon:true, type:'text', href:`${faq.url}`})
                        if note
                            .c-accordion-open.font-m-normal-14.font-w-normal-16
                                if beforeNote
                                    .c-beforeNote.font-m-normal-14.font-w-normal-16.c-hidden-mo.ccc
                                        | #{beforeNote}
                                        a(href="#").c-hidden-pc #{faq.beforeNoteLink_mo}
                                        a(href="#").c-hidden-mo #{faq.beforeNoteLink_pc}
                                        | #{faq.beforeNote2}
                                    if faq.url
                                        .button.c-cta.c-hidden-mo
                                            +linkButton({name:'Learn more', icon:true, type:'text', href:`${faq.url}`})
                                span.font-m-normal-12.font-w-normal-14 Note
                        if !note
                            .c-no-note.font-m-normal-14.font-w-normal-16
                                if beforeNote
                                    .c-beforeNote.font-m-normal-14.font-w-normal-16.c-hidden-mo
                                        | #{beforeNote}
                                        a(href="#").c-hidden-pc #{faq.beforeNoteLink_mo}
                                        a(href="#").c-hidden-mo #{faq.beforeNoteLink_pc}
                                        | #{faq.beforeNote2}
                                    if faq.url  
                                        .button.c-cta.c-hidden-mo
                                            +linkButton({name:'Learn more', icon:true, type:'text', href:`${faq.url}`})
                                //- if onlybeforeNote
                                //- span.font-m-normal-12.font-w-normal-14 Note
            +trigger(controlsId)
   
        if beforeNote
            .c-accordion__body.c-hasBeforeNote(id=`${controlsId}`)
                +contentsBody2(useImg, note, note2, faq)
        else if !note
            .c-accordion__body.c-emptyNote(id=`${controlsId}`)
        else if beforeNote2
            .c-accordion__body.c-emptyNote(id=`${controlsId}`)
        else
