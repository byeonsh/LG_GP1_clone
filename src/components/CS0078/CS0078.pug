    mixin pack2(faq, useImg, controlsId, beforeNote, note, note2)
        if faq.class
            .c-accordion__box.js-accordion-handle-target(role="listitem" class=`${faq.class}`)
                +accordionContents(faq, useImg, controlsId, beforeNote, note, note2)
        else
            .c-accordion__box.js-accordion-handle-target(role="listitem")  
                +accordionContents(faq, useImg, controlsId, beforeNote, note, note2)
                       
    mixin contentsBody2(useImg, note, note2, faq)
        // block - text contents
        if note
            .c-text-contents.c-accordion__body-text
                .c-accordion__initial.font-w-normal-20.font-m-normal-18.font-normal(aria-hidden="true") A.
                .text.c-text-contents__bodycopy.font-w-normal-16.font-m-normal-14
                    .cmp-text(data-cmp-data-layer="")
                        | #{faq.note}
                        a(href="#") #{faq.link}
                        | #{faq.note2}
       
        if useImg
            .image.c-image.c-accordion__body-image
                +printLazyImage("https://via.placeholder.com/1600x900/000/333/?text=Image", "")
   
