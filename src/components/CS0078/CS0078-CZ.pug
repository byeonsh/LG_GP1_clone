        - var option = {version: false, extension: '', hasInformation: false, reference: false, hash: 1, isSwiper: false, ...params}

        if option.isSwiper 
            .cmp-carousel__item.swiper-slide.c-carousel__item(data-cmp-data-layer="" data-cmp-hook-carousel="item")
