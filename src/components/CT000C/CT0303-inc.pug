mixin formCheckbox(label, name, val, id, count = "false", disabled = false, formChecked = false)
    .c-checkbox-item
        label.checkbox(for=`${id}`)
            if(disabled)
                input(type="checkbox", name=`${name}`, value=`${val}`, id=`${id}`, disabled="disabled")&attributes(attributes)
            else 
                if formChecked 
                    input(type="checkbox", name=`${name}`, value=`${val}`, id=`${id}` checked)&attributes(attributes)
                else
                    input(type="checkbox", name=`${name}`, value=`${val}`, id=`${id}`)&attributes(attributes)
            if(label)
                span.label!= label
                    -if(count >= 0 & count != "false") // for filter
                        sup.count= count 
            else 
                if block 
                    span.label
                        block

