mixin paymentTerms(data)
    .c-payment-terms
        each contents in data
            if contents.type === 'paypalList'
                .c-checkout-step03-payment
                    .c-payPal 
                        ul.c-payPal-list.c-payPal-list--fill
                            li Pay in 3 interest - free payment of (monetary unit) (product price / 3) with
                                span.c-payPal__bold Paypal
                                    +exclamTooltip('PayPal Pay in 3 is an interest-free loan that lets you split your basket into 3 payments, with the first due at time of purchase and subsequent payments due every month on the same date. It’s a great way to spread the cost of larger items or to cover emergencies, and is a helpful tool to manage your budget more effectively. Please note that Pay in 3 is a form of credit, so carefully consider whether you can afford the repayments and possible impact to your credit score.', false)
                                +linkButton({type:"text",color:"red", underline:true, name: "Learn more"})
                            li Pay in 3 payment methods may be restricted on some accounts.
            if contents.type === 'form'
                if contents.formTitle || contents.formText
                    .c-checkout-step03-email
                        if contents.formTitle
                            strong.c-checkout-step03-email__title= contents.formTitle
                        if contents.formText
                            +formCheckbox(contents.formText, "name", contents.formVal, contents.formId, count="false", false, contents.formChecked)
                            if contents.childForm
                                ul.childForm
                                    each childForm, i in contents.childForm
                                        +formCheckbox(childForm, "name", `${contents.formVal}-${i}`, `${contents.formVal}-${i}`, count="false", false, contents.formChecked)                                
                if contents.formList
                    ul.c-checkout-step03-email__list 
                        each list in contents.formList
                            li(class=`${contents.noMargin ? '' : 'c-checkout-step03-email__list--margin'}`) !{list}
            if contents.type === 'error'
                ul.toast-popup.c-checkout-step03-error
                    li.toast-popup__item.toast-popup__item--error
                        .toast-popup__container
                            i.toast-popup__icon.toast-popup__icon--error
                                span.sr-only= error
                            if contents.toastText
                                .toast-popup__title= contents.toastText
                        button(type='button' class='toast-popup__remove')
                            span.sr-only remove
            if contents.type === 'listBold'
                .c-checkout-step03-email
                if contents.formList
                    ul.c-checkout-step03-email__list 
                        each list in contents.formList
                            li.font-bold= list
            if contents.type === 'text'
                .c-checkout-step03-email
                    each text in contents.text
                        p.font-w-normal-16.font-m-normal-14 !{text}
            if contents.type === 'termBox'
                .c-checkout-step03-email.c-hasTermBox
                    p.font-w-normal-16.font-m-normal-14= contents.termTitle
                    .c-termBox!= contents.termText
            if contents.type === 'cardRadio'
                .card-info__box.installment-select__list
                    if contents.cardRadioSelected
                        input(type="radio" id=`cardinfo-${contents.cardVal}` name=`card-${contents.cardId}` class="installment-select__input" checked)
                    else
                        input(type="radio" id=`cardinfo-${contents.cardVal}` name=`card-${contents.cardId}` class="installment-select__input")
                    label(for=`cardinfo-${contents.cardVal}` class="installment-select__item")
                        span.installment-select__head
                            span.installment-select__info= contents.cardText
                            if contents.cardNumber
                                span.installment-select__cost= contents.cardNumber
            if contents.type === 'desc'
                .c-checkout-step03-email
                    each data in contents.desc
                        .c-text-contents
                            .text.c-text-contents__bodycopy
                                p.cmp-text.font-w-normal-16.font-m-normal-14 !{data}


mixin exclamTooltip(tooltipText, usedButton = true, direction = 'bottom-left', size = '', img = false)
    //- - var text = tooltipText || "Lorem ipsum  Neque porro quisquam est qui dolorem ipsum quia dolor sit amet, consectetur, adipisci velit.."
    if (size == 'large')
        - var size = 'c-tooltip__contents--large'
    if (size == 'medium')
        - var size = 'c-tooltip__contents--medium'
    if (size == 'auto')
        - var size = 'c-tooltip__contents--auto'
    span.c-tooltip
        span.button
            button.cmp-button.c-icon-button.c-icon-button--more-info.c-tooltip__button(type="button" aria-expanded="false", data-cmp-data-layer="")
                span.sr-only More information
        span.c-tooltip__container(class=direction)
            span.c-tooltip__contents.has-close(class=size)
                button.c-tooltip__close(type='button')
                    span.sr-only Close
                if tooltipText
                    span.c-text-contents
                        span.text.c-text-contents__bodycopy
                            span.cmp-text.font-w-normal-14.font-m-normal-14(data-cmp-data-layer="")
                                span= tooltipText
                        if usedButton
                            span.button
                                +linkButton({type:"text", color:"black", icon: true, name: "Learn More"})
                - var data = {...img}
                if img
                    .c-tooltip__img
                        each src in data
                            img(src=`../../assets/${src.route}.svg`, alt=src.alt)

