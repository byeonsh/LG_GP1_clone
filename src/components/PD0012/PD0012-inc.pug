mixin PD0012(option=false, headlineText, noneOBS=false,)
    mixin model-group(params)
        .c-product-item__groups
            - var count = params.groups.length
            - var more9 = count >= 9
            - var more6 = count >= 6
            .c-product-item__model-group(class=`group-type-${params.type}${more9 ? ' limited' : more6 ? ' limited' : ''}`)
                ul.inner(role="menu")
                    if(params.target == 'SELF')
                        case params.type
                            when 'inch'
                                - for (var x = 0; x < count; x++)
                                    li(role="menuitem")
                                        if x == count-1
                                            button.swatch(aria-label=`${params.groups[x]} inch TV` type="button" class=`${x==0 ? 'active' : ''}` disabled="disabled" aria-pressed=`${x==0 ? 'true' : 'false'}`)
                                                span(aria-hidden="true")= `${params.groups[x]}"`
                                        else
                                            button.swatch(aria-label=`${params.groups[x]} inch TV` type="button" class=`${x==0 ? 'active' : ''}` aria-pressed=`${x==0 ? 'true' : 'false'}`)
                                                span(aria-hidden="true")= `${params.groups[x]}"`
                            when 'color'
                                - for (var x = 0; x < count; x++)
                                    li(role="menuitem")
                                        button.swatch(aria-label="target color name" type="button" class=`${x==0 ? ' active' : ''}` class=`${params.groups[x]}` aria-pressed=`${x==0 ? 'true' : 'false'}`)
                                            span.sr-only(aria-hidden="true") color name
                            when 'cm'
                                - for (var x = 0; x < count; x++)
                                    li(role="menuitem")
                                        if x == count-1
                                            button.swatch(aria-label=`${params.groups[x]} cm` type="button" class=`${x==0 ? 'active' : ''}` disabled="disabled" aria-pressed=`${x==0 ? 'true' : 'false'}`)
                                                span(aria-hidden="true")= `${params.groups[x]}`
