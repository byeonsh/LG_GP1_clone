                                                span(aria-hidden="true")= `${params.groups[x]}`
                                        else    
                                            a.swatch(aria-label=`${params.groups[x]} weight` href="#" class=`${x==0 ? 'active' : ''}` data-link-name="" data-link-destination="" data-link-menu="" data-link-page-type="" aria-pressed=`${x==0 ? 'true' : 'false'}`)
                                                span(aria-hidden="true")= `${params.groups[x]}`
