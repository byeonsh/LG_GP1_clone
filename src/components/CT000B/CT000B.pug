mixin addBundlePop(id, subTitle, step, acc, item, price, unitPos, name, msg, params)
    - let data = {...params}
        //- +pop-msg-standard({id : id, headline:'', size: "full"})
        .add-bundle-title.font-w-light-36.font-m-light-24= subTitle
        .add-bundle-text.font-w-normal-16.font-m-normal-16 
            | !{step.title} 
            if step.active
                span.add-bundle-step
                    | (
                    span.add-bundle-step--active= step.active
                    | /!{step.total})
        .add-bundle-scroll
            .add-bundle-scroll__hidden
                .add-bundle-scroll__container(class=`${data[0].type === 'warranty' ? 'add-bundle-scroll__container--warranty' : ''}`)
                    if data[0].type === 'warranty'
                        .add-bundle-warranty
                            each set in data
                                +addBundleWarranty(set)
                    else
                        each set in data
                            +addBundle(set)
        // [Start] 2023-04-27 : ADD - Error toastPopup
        if msg
            +toastPopup([
                    {
                        type: msg.type,
                        text: msg.text,
                        remove: true,
                    },
                ])
        // [End] 2023-04-27 : ADD - Error toastPopup
        //- +addBundleBtn({acc: acc, item: item, price: price, unitPos: unitPos, name: name})


mixin unitPos(pos, price, unitName)
    // [Start] 2023-05-31 : UPDATE -about product price
    if pos == 'right' 
        = `${price}${unitName ? unitName : '€'}` 
    else if pos == 'sup'
        = `${price}`
        sup.c-price__unit=`${unitName ? unitName : '₫'}`
    else
        = `${unitName ? unitName : '£'}${price}`
    // [End] 2023-05-31 : UPDATE -about product price

mixin printTab(div, num, selected)
    if selected
        button.c-tabs__tab.cmp-tabs__tab.cmp-tabs__tab--active(role="tab", id=`tabs-${div}-item-${num}-tab`, aria-controls=`tabs-${div}-item-${num}-tabpanel`, tabindex="0", data-cmp-hook-tabs="tab", aria-selected="true")
                if block
                    block
    else 
        button.c-tabs__tab.cmp-tabs__tab(role="tab", id=`tabs-${div}-item-${num}-tab`, aria-controls=`tabs-${div}-item-${num}-tabpanel`, tabindex="-1", data-cmp-hook-tabs="tab", aria-selected="false")
                if block
                    block
mixin printTabPanel(div, num, selected)
