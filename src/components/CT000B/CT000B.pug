                            span.add-bundle__price(class=`${!data.sale && !data.save ? 'font-w-semibold-20 font-m-semibold-16' : 'font-w-semibold-20 font-m-semibold-16'}`)
                                +unitPos(params.unitPos, data.price)
                            if data.sale
                                span.add-bundle__sale.font-w-normal-12.font-m-normal-12
                                    +unitPos(params.unitPos, data.sale)
                            if data.save
                                span.add-bundle__save.font-w-normal-12.font-m-normal-12 Save 
                                    +unitPos(params.unitPos, data.save)
                            // [End] 2023-05-30 : UPDATE -about product price
                            if data.isSpeaker
                                // [Start] 2023-05-26 : UPDATE- change common mixin(product_rating)
                                .product-label__container
                                    +product_rating(params.productLabel, modelid='MD07546559', specsheet=true, rating=false, catalog=false, summary=false, labels=false)
