mixin pinCode(data, validation = false)
    .exchange-pincode
        .exchange-pincode-box
            each set in data
                +myFormText(set)
                .button.c-cta
                    +defaultButton({name:'Check', wSize:'medium', mSize: 'medium', buttonStyle : "default", isButton: true})
        .exchange-pincode-msg    
            if validation.correct
                +myMsg({correct: validation.correct})
            if validation.warning
                +myMsg({warning: validation.warning})
mixin exchageStepNum(id, title, content)
    .exchange-step-num(id=id)
        p.exchange-step-tit= title
        block 
mixin exchangeStep(params)
    - var config = {optionType : "", inputName : "", optionName : "", disabled : false, checked: false, ...params}
    - var hashRange = 100000
    - var hash = Math.ceil(Math.random() * hashRange).toString(16)
    - var id = `${config.inputName}-${hash}`
    - var nameHash = Math.ceil(Math.random() * hashRange)
    - var nameId = `${id}-${nameHash}`
    //- radiobox
    .each-box
        input.exchange-step-box-option__input.sr-only(type="radio" id=`${id}` name=`${config.inputName}` value=`${config.optionName}` disabled=config.disabled checked=config.checked)
        .exchange-step-box__detail
            label(for=`${id}` aria-labelledby=`${nameId}`)
            .text.c-sibling-option__name(id=`${nameId}`)
                span.cmp-text(data-cmp-text-layer)= config.optionName
mixin yourProductDetails(params)
    - const {type, size, condition, price} = params
    .exchange-step-result-box
        p.result-tit Your Product Details
        p.result-des-01 
            span.choose-result #{type} 
            span.choose-result.choose-result-two (#{size}) Television
            |  in 
            span.choose-result.choose-result-black #{condition}
            |  condition
        p.result-des-02 
            span Get up to 
            span.choose-result ₹#{price} 
            span for your exchanged product.
        p.result-des-03 Pickup and Payment will be done by 
            span.choose-result DIGI2L 
            span post inspection of your product.
