mixin myInputForm(data)
    .my-input
        ul
            each set in data
                li
                    +myFormText(set)

mixin myFormText(params)
    - let data = {...params}
    - let config = `${data.warning || data.warningFirst && !data.correct ? 'my-warn' : ''}`
    .c-input-item(class=`${data.active ? 'active' : ''}`)
        if data.label
            label.font-w-semibold-16.font-m-semibold-14(for= data.id class=`${data.info ? 'my-info' : ''}`)= data.label
        if data.required
            input(type= data.type id= data.id placeholder= data.placeholder data-placeholder=data.placeholder class=config, required)&attributes(attributes)
        else if data.type === 'textarea'
            if data.limit
                .my-textarea__container
                    textarea(id= data.id, placeholder= data.placeholder data-placeholder= data.placeholder class='my-textarea--limit')&attributes(attributes)
                    .my-count
                        span.font-w-normal-14.font-m-normal-12 0 / !{data.limit} byte
            else 
                textarea(id= data.id, placeholder= data.placeholder data-placeholder= data.placeholder)&attributes(attributes)
        else if data.type === 'select'
            +mySelect({optionVal: data.optionVal, warning: data.warning, label: data.label, id: data.id, infoText: data.infoText, placeholder: data.placeholder, warn: data.warn, selected: data.selected, tooltipMsg:data.tooltipMsg, readonly:data.readonly})
        else 
            input(type= data.type id= data.id, placeholder= data.placeholder data-placeholder= data.placeholder, value= data.value class=`my-input__password ${config}` class=`${data.search ? 'my-form__search' : ''}`, title= data.title)&attributes(attributes)
        if !data.validation 
            +myMsg({infoText: data.infoText, infoTextSecond: data.infoTextSecond, correct: data.correct, warning: data.warning, check: data.check, warningFirst: data.warningFirst})
        else
            +myMsg({infoText: data.infoText, infoTextSecond: data.infoTextSecond})

mixin mySelect({optionVal=false, warning=false, label='', id='', infoText= false, placeholder='', warn=false, selected, tooltipMsg=false, readonly=false})
    .c-search-box__form(class=`${warning ? 'my-warn' : ''}`)
        +formSelect(label, id, placeholder, false,false,false,tooltipMsg, readonly)
            if optionVal
                - for(var i=0; i < optionVal.length; i++)
                    if selected === i + 1
                        option(value="" selected)= optionVal[i]
                    else
                        option(value="")= optionVal[i]
        if warning || infoText
            +myMsg({warning: warning, infoText: infoText})


mixin myMsg({infoText= false, correct= false, warning= false, check= false, warningFirst, infoTextSecond})
    if correct
        - for(var i=0; i < correct.length; i++)
            +valid-msg(correct[i])
    if warning !==  []
        - for(var i=0; i < warning.length; i++)
            +invalid-warning(warning[i])

mixin addBundle(params)
    - let data = {...params}
    // [Start] 2023-05-30 : UPDATE -about product price
    if data.guideBar 
        .guide-text-option(style="margin:0 0 0 0") !{data.guideBar}
    // [Start] 2023-05-30 : UPDATE -about product price
    ul.add-bundle__list
        li
            .add-bundle__container 
                .add-bundle__img
                    .image.c-image
                        +printLazyImage('https://via.placeholder.com/80x80/999999/333333')
                .add-bundle__info 
                    .add-bundle__info-container
                        .add-bundle__title.font-w-normal-16.font-m-normal-14= data.title
                        .add-bundle__prices
                            //- span.add-bundle__price(class=`${!data.sale && !data.save ? 'font-w-semibold-20 font-m-semibold-16' : 'font-w-semibold-20 font-m-semibold-16'}`)= data.price
