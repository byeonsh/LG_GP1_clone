include ./CT0100-inc.pug

mixin buyProductItem(params)
    - let data = { ...params }
    .c-product-item
        .c-product-item__top-container
            .button
                +formCheckbox(false, `pdpPackageItem${data.id}`, `${data.name}`, `pdpPackageItem${data.id}`, count="false", disabled = data.outStock, labelledby = true, `aria__${data.name}`)(aria-describedby='basket_info grand_total')
                    strong.sr-only= `${data.name} Select`
            .c-product-item__visual-imagebuyProductItem
                a(href="#" data-link-name="" data-link-destination="" data-link-menu="" data-link-page-type="" data-cmp-data-layer="").image.c-image
                    //- +printLazyImage(`https://www.lg.com/uk/images/tvs/md07546594/gallery/GT1.jpg`)
                    +printLazyImage('https://via.placeholder.com/200x180/999999/000000')
        .c-product-item__bottom-container
            .c-product-item__bottom-container__text-box
                .c-text-contents.c-product-item__layout
                    .text.c-text-contents__eyebrow.sku
                        if data.name
                            .cmp-text(data-cmp-data-layer="" class="font-w-normal-16 font-m-normal-14")
                                p
                                    span= data.name
                        if data.inStock
                            span.font-w-normal-16.font-m-normal-12.font-green In Stock
                        if data.outStock
                            span.font-w-normal-16.font-m-normal-12.font-red Out of Stock
                    if data.plag
                        .text.c-text-contents__plag 
                            span.font-w-normal-16.font-m-normal-12= data.plag
                    a(href="#" data-link-name="" data-link-destination="" data-link-menu="" data-link-page-type="" data-cmp-data-layer="").text.c-text-contents__headline
                        .cmp-text(data-cmp-data-layer="" class="font-w-normal-20 font-m-normal-16") 
                            p(id=`aria__${data.name}`)= `${data.title ? `${data.title}` : 'LG B1 65 inch Class 4K Smart OLED TV w/AI ThinQ® (64.5" Diag) IntGel® core™ i7 / 16GB / 2TB / Intel®  Iris® Plus Graphics / Noble Steel / Services A'}`
                    if data.warrantyinfo 
                        .c-text-contents__warrantyinfo 
                            each set in data.warrantyinfo
                                .c-text-contents__warrantyinfo__box
                                    span.font-w-normal-16.font-m-normal-12.c-text-contents__warrantyinfo__title !{set.title} :
                                    span.font-w-normal-16.font-m-normal-12.c-text-contents__warrantyinfo__text !{set.text}
                +buyProductPrice({
                    installment: data.installment, 
                    installmentPlus: data.installmentPlus, 
                    discount: `${data.discount ? '20% off' : ''}`, 
                    purchase: `${data.purchase ? data.purchase : '1,866.98'}`, 
                    priceInstallment: data.priceInstallment, 
                    previousPrice: data.previousPrice, 
                    discount: data.discount, 
                    pricesup: data.pricesup, 
                    membership: data.membership,
                    productLabel: data.productLabel
                })
                if data.deliveryStartDate 
                    .c-product-item_delivery-status
                        span.font-w-normal-14.font-m-normal-12.c-product-item_delivery-status__text Shipment will start from
                            em.font-w-semibold-14.font-m-semibold-12.c-product-item_delivery-status__date !{data.deliveryStartDate}.
                if data.guideMsg 
                    .c-product-item_guise-msg 
                        span.font-w-normal-16.font-m-normal-12 !{data.guideMsg}
            if data.bundle
                .c-product-item__bundle
                    .c-product-item__bundle__items
                        each set in data.bundle 
                                +bundleAddItem2(set)
            .buy-product-btn
                .buy-product-btn__area
                    .buy-product-btn__pos
                        .buy-count-box(class=`${data.inStock ? 'inStock' : 'outStock'}`)
                            button(type='button' aria-describedby=`basket_info grand_total input-${data.name}`).buy-count-box__minus
                                span.sr-only minus
                            .buy-count-box__number
                                input(type='number' value= '1' title="Quantity" id=`input-${data.name}`)
                            button(type='button' aria-describedby=`basket_info grand_total input-${data.name}`).buy-count-box__plus.active
                                span.sr-only plus
                            if data.outStock 
                                a.cmp-button.c-button.c-button--text-underline.black.js-pop-open(href="#" role="button")
                                    span.cmp-button__text.c-button__text Get Stock Alert
                        // [Start] 2023-11-27 : ADD-cart subTotal area
                        if data.subTotalText && data.subTotalPrice
                            .c-product-item__sub-total
                                .c-text-contents.c-product-item__sub-total__price
                                    .c-text-contents__bodycopy
                                        span.cmp-text.font-w-semibold-32.font-m-semibold-24(data-cmp-data-layer="") !{data.subTotalText}
                                        span.cmp-text.font-w-semibold-32.font-m-semibold-24(data-cmp-data-layer="") !{data.subTotalPrice}
                                if data.subTotalLink
                                    +textButton({name: `${data.subTotalLink}`, wSize: 'medium', mSize: 'small', buttonStyle: 'default', underline : true, isButton: false, mainClass: 'js-pop-open'})
                        // [End] 2023-11-27 : ADD-cart subTotal area
                    button(type="button" class='c-product-item__input--delete' aria-describedby=`basket_info grand_total input-${data.name}`)
                        span.sr-only Delete 

mixin buyProductPrice(params)
    - var priceStyle = params.priceFontStyle || "font-w-semibold-32 font-m-semibold-24"
    - var subPriceStyle = params.subPriceFontStyle || "font-w-normal-14i font-m-normal-12"
    - var soldOutStyle = params.soldOutFontStyle || "font-w-semibold-18 font-m-semibold-18"
    .price-area
        if(params.promotion)
            .price-box(class=`${subPriceStyle}`)
                .c-price__flag--promotion(class=params.promotion=="Limited Quantities" ? "limited" : "")
                    span= params.promotion
        if(params.installment)
            .price-box(class=`${subPriceStyle}`)
                .c-price
                    if(typeof params.installment === "number")
                        - for(let i = 0; i < params.installment; i++)
                            .c-price__installment.c-product-item__button
                                a(href="#" role="button") O S/ 583.25/al mes. 12 cuotas con tasa 0%
                    else if params.installmentText
                        .c-price__installment.c-product-item__button
                            a(href="#" role="button")= params.installmentText
                    else if params.installmentTag
                        .c-price__installment.c-product-item__button !{params.installmentTag}
                    else
                        .c-price__installment.c-product-item__button
                            a(href="#" role="button") O S/ 583.25/al mes. 12 cuotas con tasa 0%
        .price-box 
            .c-price(class=`${priceStyle}`)
                if (params.pricesup)
                    .c-price__purchase
                        span= params.pricesup
                            sup.c-price__unit ₫
                else
                    .c-price__purchase= params.purchase
            //- [Start] 2023-09-13 : ADD- Includes $15 EHF fee
            if params.includeEHF
                span.c-price__EHF-fee.font-w-normal-16.font-m-normal-12 !{params.includeEHF}
            //- [End] 2023-09-13 : ADD- Includes $15 EHF fee
            if (params.productLabel)
                .product-label__container
                    +product_rating(params.productLabel, modelid='MD07546559', specsheet=true, rating=false, catalog=false, summary=false, labels=false)
            
            .product-price(class=`${subPriceStyle}`)
                .c-price
                    //- .c-price__previous= params.previousPrice
                    .c-price__previous= params.previousPrice
                        if (params.previousPrice && params.pricesup)
                            sup.c-price__unit ₫
                .c-price
                    if (params.discount)
                        .c-price__discount 
                            span Save
                            span= params.discount
                                if (params.discount && params.pricesup)
                                    sup.c-price__unit ₫
        if (params.installmentPlus)
            .price-box(class=`${subPriceStyle}`)
                .c-price__installmentPlus
                    span= params.installmentPlus
        if(typeof params.membership === "object")
            each item in params.membership
                .price-box(class=`${subPriceStyle}`)
                    .c-price__flag--member
                        span.flag= item.title
                        span= item.cost
        else
            if(params.membership)
                .price-box(class=`${subPriceStyle}`)
                    .c-price__flag--member
                        span.flag LG Members only
                        span= params.membership

        if(params.outOfStock || params.soldOut)    
            .price-box(class=`${soldOutStyle}`)
                if(params.outOfStock)
                    .c-price__flag--notice
                        span Out of stock
                if(params.soldOut)    
                    .c-price__flag--notice
                        span Sold out

mixin ctWarrantyAdd(params)
    each data in params
        .ct-warranty
            if !data.items
                .ct-warranty-add
                    .c-text-contents
                        .c-text-contents__title
                            span.font-w-semibold-16.font-m-semibold-14 Buy Together
                            +button({type:"text", underline:true, color:"black", name:`${data.buttonName}`})
            if data.items
                .ct-warranty-add
                    .c-text-contents
                        .c-text-contents__title
                            span.font-w-semibold-16.font-m-semibold-14 Buy Together
                            +button({type:"text", underline:true, color:"black", name:`${data.buttonName}`})
                    each set in data.items
                        .ct-warranty-add__item
                            .image.c-image
                                +printLazyImage(`https://via.placeholder.com/200x200/999999/333333`)
                            .c-text-contents
                                .text.c-text-contents__eyebrow
                                    if set.code 
                                        span.font-w-normal-12.font-m-normal-12.font-regular= set.code
                                    if set.inStock
                                        span.font-w-semibold-12.font-m-semibold-12.font-green In Stock
                                    if set.outStock
                                        span.font-w-semibold-12.font-m-semibold-12.font-red Out of Stock
                                    if (set.code || set.inStock || set.outStock)
                                        .button.ct-warranty-add__edit
                                            // [Start] 2023-04-10 : REMOVE - Edit button		
                                            //- a.cmp-button.c-button.c-button--text-underline.black.js-pop-open(href="#" role="button")
                                            //-     span.cmp-button__text.c-button__text Edit
                                            // [End] 2023-04-10 : REMOVE - Edit button		
                                            +linkButton({href:"#", type:"text", class:"js-pop-open", underline:"true", name:"Edit", roleButton:"true"})
                                .c-text-contents__title
                                    span.font-w-normal-16.font-m-normal-14= set.title
                                    if !(set.code || set.inStock || set.outStock)
                                        .button.ct-warranty-add__edit
                                            // [Start] 2023-04-10 : REMOVE - Edit button		
                                            //- a.cmp-button.c-button.c-button--text-underline.black.js-pop-open(href="#" role="button")
                                            //-     span.cmp-button__text.c-button__text Edit
                                            // [End] 2023-04-10 : REMOVE - Edit button		
                                            +linkButton({href:"#", type:"text", class:"js-pop-open", underline:"true", name:"Edit", roleButton:"true"})

                                .ct-warranty-add__price-area
                                    // [Start] 2023-05-30 : UPDATE -about product price
                                    if set.pricePurchase
                                        .ct-warranty-add__price.ct-warranty-add__price--purchase
                                            if set.isOnlyPrice
                                                //- span.font-w-semibold-20.font-m-semibold-16 !{set.pricePurchase}
                                                span.font-w-semibold-20.font-m-semibold-16 
                                                    +unitPos(set.unitPos, set.pricePurchase, set.unitName)
                                            else
                                                //- span.font-w-semibold-20.font-m-semibold-16 !{set.pricePurchase}
                                                span.font-w-semibold-20.font-m-semibold-16 
                                                    +unitPos(set.unitPos, set.pricePurchase, set.unitName)
                                    if set.pricePrevious
                                        .ct-warranty-add__price.ct-warranty-add__price--previous
                                            //- span.font-w-normal-12.font-m-normal-12 !{set.pricePrevious}
                                            span.font-w-normal-12.font-m-normal-12 
                                                +unitPos(set.unitPos, set.pricePrevious, set.unitName)
                                    if set.priceDiscount
                                        .ct-warranty-add__price.ct-warranty-add__price--discount
                                            //- span.font-w-normal-12.font-m-normal-12 Save !{set.priceDiscount}
                                            span.font-w-normal-12.font-m-normal-12 Save 
                                                +unitPos(set.unitPos, set.priceDiscount, set.unitName)
                                    // [End] 2023-05-30 : UPDATE -about product price
                                if set.warning
                                    .c-text-contents__bodycopy.ct-warranty-warning
                                        p.font-w-normal-12.font-m-normal-12.font-red !{set.warning}
                                .c-text-contents__bottom
                                    if set.qty
                                        .c-text-contents__bodycopy
                                            span.font-w-normal-14.font-m-normal-14.font-black Qty: !{set.qty}
                                    button(type="button" class='c-product-item__input--delete')
                                        span.sr-only Delete

