mixin bundleAddItem(params)
    - let data={...params}
        li
            div
                .my-bundle-add__input
                    +formCheckbox(false, `pdpPackageItem${data.id}`, `${data.name}`, `pdpPackageItem${data.id}`, count="false", disabled = data.outStock)(aria-describedby='basket_info grand_total')
                        strong.sr-only= `${data.name} Select`
                .my-bundle-add__box
                    .my-bundle-add__imgbox
                        +printLazyImage(`https://via.placeholder.com/80x80/999999/333333`)
                    .my-bundle-add__txtbox
                        .text.c-text-contents__eyebrow.sku
                            +product_name(`${data.name}`,)
                            .c-text-contents__bodycopy
                                if data.inStock
                                    span.font-w-normal-16.font-m-normal-12.font-green IN STOCK
                                if data.outStock
                                    span.font-w-normal-16.font-m-normal-12.font-red OUT OF STOCK
                        p.font-w-semibold-16.font-m-semibold-14.my-bundle-add__bundle= data.title
                        +product_price({promotion: data.promotion, installment:data.installment, discount: data.discount, pricesup: data.pricesup, membership: data.member, installmentPlus:data.installmentPlus})
                        if data.desc
                            each set in data.desc
                                .my-bundle-add__desc
                                    span.font-w-normal-14.font-m-normal-14.font-red= set.desc
                .buy-product-btn
                    .buy-count-box(class=`${data.inStock ? 'inStock' : 'outStock'}`)
                        button(type='button' aria-describedby=`basket_info grand_total input-${data.name}-${data.id}`).buy-count-box__minus
                            span.sr-only minus
                        .buy-count-box__number
                            input(type='number' value= '1' id=`input-${data.name}-${data.id}`)
                        button(type='button').buy-count-box__plus.active
                            span.sr-only plus
                    button(type="button" class='c-product-item__input--delete' aria-describedby=`basket_info grand_total input-${data.name}-${data.id}`)
                        span.sr-only Delete

