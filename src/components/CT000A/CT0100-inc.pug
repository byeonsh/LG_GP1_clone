mixin buyProductPrice(params)
    - var priceStyle = params.priceFontStyle || "font-w-semibold-32 font-m-semibold-24"
    - var subPriceStyle = params.subPriceFontStyle || "font-w-normal-14i font-m-normal-12"
    - var soldOutStyle = params.soldOutFontStyle || "font-w-semibold-18 font-m-semibold-18"
    - var subBenefitStyle = params.subBenefitFontStyle || "font-w-normal-16 font-m-normal-12"

    if (params.productLabel)
        // [Start] 2023-05-26 : UPDATE- UPDATE- Change markup level, change common mixin(product_rating)
        .product-label__container
            +product_rating(params.productLabel, modelid='MD07546559', specsheet=true, rating=false, catalog=false, summary=false, labels=false)
        // [End] 2023-05-26 : UPDATE- UPDATE- Change markup level, change common mixin(product_rating)
    .price-area
        if(params.promotion)
            .price-box(class=`${subPriceStyle}`)
                .c-price__flag--promotion(class=params.promotion=="Limited Quantities" ? "limited" : "")
                    span= params.promotion
        if(params.vipPremierBenefit)
            .price-box
                .c-price
                    ul.c-price__vipPremierBenefit-list
                        each setData in params.vipPremierBenefit
                            li.font-red(class=`${subBenefitStyle}`)
                                span= setData
        if(params.installment)
            .price-box(class=`${subPriceStyle}`)
                .c-price
                    if(typeof params.installment === "number")
                        - for(let i = 0; i < params.installment; i++)
                            .c-price__installment.c-product-item__button
                                a(href="#" role="button") 699.99/mo for 3 mos at 0% or
                    else if params.installmentText
                        each data in params.installmentText
                            .c-price__installment.c-product-item__button
                                a(href="#" role="button")= data
                    else if params.installmentTag
                        .c-price__installment.c-product-item__button !{params.installmentTag}
                    else if params.installmentImgText    
                        each data in params.installmentImgText  
                            .c-price__installment.c-product-item__button
                                if data.installmentAddClass
                                    if data.installmentAddClass == 'payment_zippay_logo'
                                        a(href="#" role="button")
                                            span.payment_zippay= data.installmentText 
                                                span.image-box.c-image-installment.payment_zippay_logo
                                    else if data.installmentAddClass == 'payment_afterpay_logo'
                                        a(href="#" role="button")
                                            span.payment_afterpay= data.installmentText 
                                                span.image-box.c-image-installment.payment_afterpay_logo
                                    else
                                        a(href="#" role="button")= data.installmentText 
                                            span.image-box.c-image-installment(class=`${data.installmentAddClass}`)
                                else
                                    a(href="#" role="button")= data.installmentText
                                        if data.installmentImg
                                            span.image.c-image-installment
                                                img(src=`${data.installmentImg}`)
                    else
                        .c-price__installment.c-product-item__button
                            a(href="#" role="button") 699.99/mo for 3 mos at 0% or
        if(params.previousPriceVN)
            .product-price.font-w-normal-14i.font-m-normal-14
                .c-price
                    .c-price__previous= params.previousPriceVN
                        if (params.previousPriceVN && params.pricesup)
                            sup.c-price__unit ₫
        .price-box 
            .c-price(class=`${priceStyle}`)
                // [Start] 2023-05-30 : UPDATE -about product price
                if (params.pricesup)
                    .c-price__purchase
                        span= params.pricesup
                            sup.c-price__unit ₫
                else
                    .c-price__purchase= params.purchase
                // [End] 2023-05-30 : UPDATE -about product price
            //- [Start] 2023-09-13 : ADD- Includes $15 EHF fee
            if params.includeEHF
                span.c-price__EHF-fee.font-w-normal-16.font-m-normal-12 !{params.includeEHF}
            //- [End] 2023-09-13 : ADD- Includes $15 EHF fee
            if(!params.previousPriceVN && !params.discountVN)
                .product-price(class=`${subPriceStyle}`)
                    .c-price
                        // [Start] 2023-05-30 : UPDATE -about product price
                        //- .c-price__previous= params.previousPrice
                        .c-price__previous= params.previousPrice
                            if (params.previousPrice && params.pricesup)
                                sup.c-price__unit ₫
                        // [End] 2023-05-30 : UPDATE -about product price
                    .c-price
                        if (params.discount)
                            .c-price__discount 
                                span Save
                                span= params.discount
                                    // [Start] 2023-05-30 : UPDATE -about product price
                                    if (params.discount && params.pricesup)
                                        sup.c-price__unit ₫
                                    // [Start] 2023-05-30 : UPDATE -about product price
        if(params.discountVN)
            .product-price.font-w-normal-14i.font-m-normal-14
                .c-price
                    if (params.discountVN)
                        .c-price__discount 
                            span= params.discountVN
                                if (params.discountVN && params.pricesup)
                                    sup.c-price__unit ₫
        if(params.installmentBottom)
            .price-box(class=`${subPriceStyle}`)
                .c-price
                    if(typeof params.installmentBottom === "number")
                        - for(let i = 0; i < params.installmentBottom; i++)
                            .c-price__installment.c-product-item__button
                                a(href="#" role="button") Chỉ 1.412.016 VNĐ/tháng(12 tháng) với 0% lãi suất
                    else if params.installmentText
                        .c-price__installment.c-product-item__button
                            a(href="#" role="button")= params.installmentText
                    else if params.installmentTag
                        .c-price__installment.c-product-item__button !{params.installmentTag}
                    else
                        .c-price__installment.c-product-item__button
                            a(href="#" role="button") Chỉ 1.412.016 VNĐ/tháng(12 tháng) với 0% lãi suất
        if (params.installmentPlus)
            .price-box(class=`${subPriceStyle}`)
                .c-price__installmentPlus
                    span= params.installmentPlus
        if(typeof params.membership === "object")
            each item in params.membership
                .price-box(class=`${subPriceStyle}`)
                    .c-price__flag--member
                        span.flag= item.title
                        span= item.cost
        else
            if(params.membership)
                .price-box(class=`${subPriceStyle}`)
                    .c-price__flag--member
                        span.flag LG Members only
                        span= params.membership

        if(params.outOfStock || params.soldOut)    
            .price-box(class=`${soldOutStyle}`)
                if(params.outOfStock)
                    .c-price__flag--notice
                        span Out of stock
                if(params.soldOut)    
                    .c-price__flag--notice
                        span Sold out

mixin ctWarrantyAdd(controlsId, selectionSkuView, service)
    include ../PD0003/_summary-information.pug
    .ct-warranty
        .ct-warranty-add
            // [Start] 2023-08-23 : Changed - Add on bundle		
            .c-accordion.js-accordion
                +product-sibling-option-box({title : "Buy More, Save More", controlsId : controlsId, columnCount : 1, useTooltip : true})
                    // step 1: c-sibling-option-box__option-group
                    // step 2: c-sibling-option-box__option-group c-addon-product-box--option-choosing
                    // step 3: c-sibling-option-box__option-group c-addon-product-box--option-selected
                    .c-sibling-option-box__option-group
                        // bundle (step1 sample)
                        .c-addon-product-box.c-accordion__box
                            // before selection
                            +product-addon-before('bundleBtype')
                            // choosing
                            +product-addon-selection('bundleBtype', selectionSkuView)
                            // selection done
                            +product-addon-after('bundleBtype')

                        // accessory
                        .c-addon-product-box.c-accordion__box
                            // before selection
                            +product-addon-before('accessoryBtype')
                            // choosing
                            +product-addon-selection('accessoryBtype', selectionSkuView)
                            // selection done
                            +product-addon-after('accessoryBtype')

                        // warranty
                        .c-addon-product-box.c-accordion__box
                            // before selection
                            +product-addon-before('warranty')
                            // choosing
                            +product-addon-selection('warranty')
                            // selection done
                            +product-addon-after('warranty')
                        if (service)
                          // service
                          .c-addon-product-box.c-accordion__box
                              // before selection
                              +product-addon-before('service')
                              // choosing
                              +product-addon-selection('service')
                              // selection done
                              +product-addon-after('service')
            // [End] 2023-08-23 : Changed - Add on bundle

