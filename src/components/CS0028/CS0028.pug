    // 20230315 markup change(tag change)

    mixin specSheetItem(extension, fileName)
        .c-resources__item
            if(extension == 'pdf' || extension == 'txt' || extension == 'djvu' || extension == 'zip' || extension == 'html' || extension == 'dwg' || extension == '')
                div(class=`c-resources__item--extension c-resources__item--extension-${extension}`)
                    span.sr-only= `extension : ${extension}`
            else
                div(class=`c-resources__item--extension c-resources__item--extension-etc`)
                    span.sr-only= `extension : ${extension}`
            .c-resources__item--download
                .c-resources__item--download-info
                    a(href="#" role="download" aria-label="").c-resources__item--download-info-name.font-underline 
                        span= `${fileName}`
                        .c-resources__item--download-button.c-resources__item--download-button--icon
                            .c-icon-button.c-icon-button--download

    mixin buttonVersionItem2(params)
        - var option = {version: false, extension: '', hasInformation: false, reference: false, hash: 1, isSwiper: false, ...params}

        if option.isSwiper 
            .cmp-carousel__item.swiper-slide.c-carousel__item(data-cmp-data-layer="" data-cmp-hook-carousel="item" role='listitem')
                .c-resources__item
                    div(class=`c-resources__item--extension c-resources__item--extension-etc`)
                        +printLazyPicture('https://via.placeholder.com/40x40/ddd/eee/?text=40x40', 'https://via.placeholder.com/64x64/ddd/eee/?text=64x64')
                    .c-resources__item--download
                        .c-resources__item--download-info
                            span.c-resources__item--download-info-name.font-w-normal-24.font-m-normal-16 GSXV90MCDE
                            .c-resources__item--download-info-wrapper.font-w-normal-12.font-m-normal-12                                
                                .c-resources__item--download-info-size
                                    span LG InstaView Door-in-Door™ 

                        .c-resources__item--download-button.c-hidden-mo
                            +linkButton({type:"text", color:"black", size:"small", name: "View product support"})
        else                     
            li.c-resources__item
                if(option.version) 
                    p.c-resources__version.font-m-normal-16.font-w-normal-16 [Program/Win10_64bit] LG Update Center for LG Notebook.(Ver.1.0.1712.1201)
                if(option.extension == 'pdf' || option.extension == 'txt' || option.extension == 'djvu' || option.extension == 'zip' || option.extension == 'html' || option.extension == 'dwg')
                    div(class=`c-resources__item--extension c-resources__item--extension-${option.extension}`)
                        span.sr-only= `extension : ${option.extension}`
                else
                    div(class=`c-resources__item--extension c-resources__item--extension-etc`)
                        span.sr-only= `extension : ${option.extension}`
                .c-resources__item--download
                    .c-resources__item--download-info
                        a(href="#" role="download" id="label_id")
                            span.c-resources__item--download-info-name.font-w-regular-16.font-m-regular-16= `English.${option.extension}`
                        if(option.hasInformation)
                            .c-resources__item--download-info-wrapper.font-w-normal-14.font-m-normal-14
                                if(option.reference)
                                    .c-resources__item--download-info-ref
                                        a(href=`#popReference${option.hash}` type="button" class="js-pop-open"  title="Layer popup opens") Reference

                                .c-resources__item--download-info-size
                                    span 1,923 KB
                                .c-resources__item--download-info-date
                                    span 14/12/2021
                    .c-resources__item--download-button
                        if option.extension == 'html'
                            +buttonIcon({type:"open"})(aria-describedby="label_id")
                        else   
                            +buttonIcon({type:"download"})(aria-describedby="label_id")

