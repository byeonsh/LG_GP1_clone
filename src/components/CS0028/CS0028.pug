    mixin featured-info({mobileType = "block", initialClass = "bg-transparent font-black", count = 2, items})
        // block : featured-info
        if "swiper" == mobileType 
            .c-spec-info.swiper-mobile
                .c-spec-info__container
                    .carousel.panelcontainer
                        // block : carousel
                        .cmp-carousel.c-carousel.c-icon-block-contents.type-b(data-cmp-data-layer="", class=`${initialClass}`)
                            .cmp-carousel__content.swiper-wrapper
                                // slides
                                +benefitContents({
                                    isSwiper: true,
                                    count
                                })
                            // indicator
                            .c-carousel__handler
                                .cmp-carousel__actions
                                    +carouselBtn({type:"pagination", direction:"prev"})(aria-label="")
                                +carouselPagination
                                .cmp-carousel__actions
                                    +carouselBtn({type:"pagination", direction:"next"})(aria-label="")

        if "block" == mobileType 
            .c-spec-info.block-mobile
                .c-icon-block-contents.type-a(class=`${initialClass}`)
                    if "cs-contents" == featured
                        // block : text-contents
                        +featuredContents
                    else 
                        //- +benefitContents(4)
                        // 20230306 new design markup change
                        - var option={isSwiper: false, count, ...params}
                            - for (var x = 0; x < option.count; x++)
                                a(href="#").c-featured__wrap 
                                    span 
                                        span.c-featured__tit
                                            // Add an icon image
                                            .cmp-title(class=headlineClass, data-cmp-data-layer="")
                                                strong.cmp-title__text.font-m-normal-16.font-w-light-36 #{items.title}
                                                // Deletion of eybrow (display: none)
                                                //- .c-text-contents__eyebrow(class=option.isSwiper ? "font-m-semibold-18" : "font-m-semibold-16") Type
                                            span.c-featured__desc
                                                .c-text-contents__bodycopy.font-m-normal-12.font-w-normal-16 #{items.desc}
                                        +printLazyImage(`${items.moImg}`, "Smart Diagnosis")(aria-hidden="true")
                                        // Add button
                                    span.button.c-cta.c-hidden-mo
                                        +buttonArrow({link:false, color:'black', border:'yes', wsize:'large', msize:'small'}) 

