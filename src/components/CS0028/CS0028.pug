    // 20230315 button markup change
    mixin buttonIcon(params) 
        - var option={...params}
        - var className = `c-btn-arrow c-btn-${option.type}`
        if(option.type == 'download')   
            a(href="#" class=`${className}` role="download" data-link-name data-link-destination data-link-menu data-link-page-type)&attributes(attributes)
                span.sr-only Download
        else
            a(href="#" class=`${className}` target="_blank" data-link-name data-link-destination data-link-menu data-link-page-type)&attributes(attributes)
                span.sr-only Opens in a new window

