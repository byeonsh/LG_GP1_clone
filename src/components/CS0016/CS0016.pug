extends ../_common/_layout.pug
include ../_common/_button.pug
include ../_common/_headline.pug
include ../_common/_image.pug

block meta 
    - var componentId = "CS0016"
    - var componentName = "Motion Slide"
    - var description = "description text"
    - var keywords = "keyword list"
    title #{componentId}. #{componentName}
    meta(name="description" content=description)
    meta(name="keywords" content=keywords)

block styles
    link(rel="stylesheet", href=componentId+".css")

block scripts
    //- script(src=componentId+".js")
    script(src=componentId+".js" type="module")

block content
    // An area for testing options when coding. 
    // Please do not use it during development.
    .guide 
        .grid-1600 
        .grid-1440
        .inner
            .guide-list
                a(href="../../list-st.html") List 
            .guide-title #{componentId}. #{componentName}
            .guide-wrapper
                script. 
                    const guideList = {
                        '.c-wrapper' : [
                            {
                                'name' : 'BG Color',
                                'buttons' : [
                                    {'toggle' : false, 'add' : 'bg-warmgray', 'remove' : 'bg-white, bg-lightgray, bg-darkgray, bg-black'},
                                    //- {'toggle' : false, 'add' : 'bg-white', 'remove' : 'bg-default, bg-lightgray, bg-darkgray, bg-black'},
                                    //- {'toggle' : false, 'add' : 'bg-lightgray', 'remove' : 'bg-default, bg-white, bg-darkgray, bg-black'},
                                    //- {'toggle' : false, 'add' : 'bg-darkgray', 'remove' : 'bg-default, bg-white, bg-lightgray, bg-black'},
                                    {'toggle' : false, 'add' : 'bg-black', 'remove' : 'bg-default, bg-white, bg-lightgray, bg-darkgray'},
                                ]
                            }
                        ],                     
                        '.c-text-contents__bodycopy, .c-text-contents__disclaimer' : [
                            {
                                'name' : 'Font Options',
                                'buttons' : [
                                    {'toggle' : true, 'add' : 'font-italic'},
                                    {'toggle' : true, 'add' : 'font-underline'},
                                ]
                            }
                        ],
                    };

    // Component Markup

    .guide-text-type Motion Slide 
    // Option. black add class .bg-black
    .c-wrapper.CS0016.type-text.bg-black
        .component.full.c-wide-full.c-with-thinq
            .CS0016__container
                // 20230309 change markup
                .c-header-wrap
                    // c-cs-headr, headline, bodybopy Optionally available. -> Delete (or hide) the tag
                    .c-cs-header
                        // block : text-contents
                        .c-text-contents
                            .title.c-text-contents__headline
                                +headline('font-w-light-56 font-m-light-28',h2,'Get going with ThinQ')
                            .text.c-text-contents__bodycopy
                                .cmp-text.font-w-normal-20.font-m-normal-16(data-cmp-data-layer="")
                                    span Manage all your devices from one place whether from home, on the go, or lounging by the beach.<br>Press the plus button to see how you can install the app.
                    .button
                        +defaultButton({name: 'Google Play', wSize: 'large', mSize: 'medium', buttonStyle: 'default'})
                        +defaultButton({name: 'Apple App Store', wSize: 'large', mSize: 'medium', buttonStyle: 'default'})


                .c-motion-slide
                    .c-motion-slide__title
                        p.font-m-normal-20.font-w-normal-32 How to install the LG ThinQ app  
                        .c-motion-slide__nav-area
                            .swiper-button-prev(role="button") prev
                            .swiper-button-next(role="button") next

                    .c-motion-slide__top.swiper                        
                        .swiper-wrapper 
                            .c-motion-slide__item.swiper-slide(tabindex=0)
                                //- +printLazyPicture("https://via.placeholder.com/138x138/fff/333/?text=Image1", "https://via.placeholder.com/430x430/fff/333/?text=Image1")
                                +printLazyPicture("../../mocks/CS0016/temp-thinq.jpg", "")
                            .c-motion-slide__item.swiper-slide(tabindex=0) 
                                +printLazyPicture("https://via.placeholder.com/138x138/fff/333/?text=Image2", "https://via.placeholder.com/430x430/fff/333/?text=Image2")
                            .c-motion-slide__item.swiper-slide(tabindex=0) 
                                +printLazyPicture("https://via.placeholder.com/138x138/fff/333/?text=Image3", "https://via.placeholder.com/430x430/fff/333/?text=Image3")
                            .c-motion-slide__item.swiper-slide(tabindex=0)
                                +printLazyPicture("https://via.placeholder.com/138x138/fff/333/?text=Image4", "https://via.placeholder.com/430x430/fff/333/?text=Image4")
                            .c-motion-slide__item.swiper-slide(tabindex=0)
                                +printLazyPicture("https://via.placeholder.com/138x138/fff/333/?text=Image5", "https://via.placeholder.com/430x430/fff/333/?text=Image5")

                    .c-motion-slide__bottom.align-text-center
                        .c-motion-slide__nav.swiper
                            .swiper-wrapper
                                .c-motion-slide__step-list.swiper-slide
                                    dl.c-motion-slide__step-text 
                                        dt.c-motion-slide__headline.font-m-light-24.font-w-light-36 Step 1. 
                                            br
                                            | ThinQ app download
                                        dd.c-motion-slide__bodycopy.font-m-normal-12.font-w-normal-20 Search for the LG ThingQ application from the Google Play or Apple App Store on a smart phone.
                                .c-motion-slide__step-list.swiper-slide
                                    dl.c-motion-slide__step-text 
                                        dt.c-motion-slide__headline.font-m-light-24.font-w-light-36 Step 2.
                                            br
                                            | Log-in
                                        dd.c-motion-slide__bodycopy.font-m-normal-12.font-w-normal-20 Log in with your LG account if you have one.
                                .c-motion-slide__step-list.swiper-slide
                                    dl.c-motion-slide__step-text 
                                        dt.c-motion-slide__headline.font-m-light-24.font-w-light-36 Step 3.
                                            br
                                            | Add a device
                                        dd.c-motion-slide__bodycopy.font-m-normal-12.font-w-normal-20 You made it to LG ThinQ app's main page! Now time to commect your LG device(s).
                                .c-motion-slide__step-list.swiper-slide
                                    dl.c-motion-slide__step-text 
                                        dt.c-motion-slide__headline.font-m-light-24.font-w-light-36 Step 4.
                                            br
                                            | Select a device
                                        dd.c-motion-slide__bodycopy.font-m-normal-12.font-w-normal-20 Select which device you want to connect to.
                                .c-motion-slide__step-list.swiper-slide
                                    dl.c-motion-slide__step-text 
                                        dt.c-motion-slide__headline.font-m-light-24.font-w-light-36 Step 5.
                                            br
                                            | Go ahead!
                                        dd.c-motion-slide__bodycopy.font-m-normal-12.font-w-normal-20 Access your device(s) using the ThinQ app.


