    mixin contentsBody2(useImg, note, faq)
        // block - text contents
        if note
            .c-text-contents.c-accordion__body-text
                .c-accordion__initial.font-w-normal-20.font-m-normal-18.font-normal(aria-hidden="true") A.
                .text.c-text-contents__bodycopy.font-w-normal-16.font-m-normal-16
                    .cmp-text(data-cmp-data-layer="") 
                        ul.font-m-normal-16.font-m-normal-14
                            each val in note
                                li= val
        if useImg
            .image.c-image.c-accordion__body-image
                +printLazyImage("https://via.placeholder.com/1600x900/000/333/?text=Image", "")

    mixin contentsBody(useImg, faq, item)
        // block - text contents
        .c-text-contents.c-accordion__body-text

            span.c-accordion__initial.font-w-normal-20.font-m-normal-18.font-normal(aria-hidden="true") A.
            .text.c-text-contents__bodycopy.font-w-normal-16.font-m-normal-16
                span.cmp-text(data-cmp-data-layer="") 
                div
                    each val in item
                        dl
                            dt.font-m-normal-14.font-w-normal-14.c-cruise-title Country / Port
                            dd.font-m-normal-16.font-w-normal-20= val.country
                            dt.font-m-normal-14.font-w-normal-14.c-cruise-title Sub. contact point
                            dd.font-m-normal-14.font-w-normal-16.c-cruise-contact #{val.contact}
                            dd.font-m-normal-14.font-w-normal-16.c-cruise-contact #{val.contact2}
                            dd.font-m-normal-14.font-w-normal-16.c-cruise-contact #{val.contact3}
                            dd.font-m-normal-14.font-w-normal-16.c-cruise-contact #{val.contact4}
                            dt.font-m-normal-14.font-w-normal-14.c-cruise-title Email
                            dd.font-m-normal-14.font-w-normal-16.c-cruise-email #{val.email}
                            dd.font-m-normal-14.font-w-normal-16.c-cruise-email #{val.email2}
                            dd.font-m-normal-14.font-w-normal-16.c-cruise-email #{val.email3}
                            dt.font-m-normal-14.font-w-normal-14.c-cruise-title 备注
                            dd.font-m-normal-14.font-w-normal-16.c-cruise-remark #{val.remark}
                            dd.font-m-normal-14.font-w-normal-16.c-cruise-remark #{val.remark2}
                            dd.font-m-normal-14.font-w-normal-16.c-cruise-remark #{val.remark3}
