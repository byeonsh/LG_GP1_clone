extends ../_common/_layout.pug
include ../_common/_headline.pug
include ../_common/_button.pug

mixin regionList(title, params)
    -let data = { ...params }
    .region
        .region__title 
            span.font-w-normal-24.font-m-normal-20= title
        .region__area 
            .region__list
                each set in data
                    span.region__list__item
                        +textButton({name: set, underline:true, buttonStyle: 'default', isButton: false})

mixin deliveryFee
    .c-wrapper
        .component
            .cmp-container
                .delivery-fee
                    .delivery-fee__wrap
                        +headline("font-w-light-56 font-m-light-28", false, "遠距離配送追加費用")
                        .delivery-fee__area 
                            +regionList("北海道", ["北海道"])
                            +regionList("東北", ["青森県", "秋田県", "岩手県", "宮城県", "山形県", "福島県"])
                            +regionList("関東", ["茨城県", "栃木県", "群馬県", "千葉県", "神奈川県"])
                            +regionList("中部", ["新潟県", "富山県", "石川県", "福井県", "山梨県", "長野県", "岐阜県", "静岡県", "愛知県"])
                            +regionList("近畿", ["三重県", "滋賀県", "京都府", "兵庫県", "奈良県", "和歌山県"])
                            +regionList("中国", ["鳥取県", "島根県", "岡山県", "広島県", "山口県"])
                            +regionList("四国", ["徳島県", "香川県", "愛媛県", "高知県"])
                            +regionList("九州・沖縄", ["熊本県"])
                        .delivery-fee__description 
                            p(class="font-w-normal-16 font-m-normal-12") ※ 離島は配送対象外となります。

mixin deliveryDetail(data)
    -let region = data 
    .c-wrapper 
        .component 
            .cmp-container 
                .delivery-fee
                    .delivery-fee__wrap
                        .delivery-fee__back
                            +backButton
                        +headline("font-w-light-56 font-m-light-28", false, region)
                        .delivery-fee__area 
                            +feeTable(region)

mixin backButton 
    button.cmp-button(type="button" aria-controls="" data-cmp-data-layer="")
        span.cmp-button__text.sr-only back



