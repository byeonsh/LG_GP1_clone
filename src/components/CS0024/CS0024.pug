    mixin listLinkResetArticle(text)
        li.c-list-link__item
            a(href="#" data-link-name data-link-destination data-link-menu data-link-page-type)
                .c-list-link__eyebrow.font-m-normal-14.font-w-semibold-18= text.eyebrow
                .c-list-link__headline.font-w-normal-20.font-m-normal-18= text.headline

    mixin listLinkArticles(list)
        - var lengthMobile = 3;
        - var lengthDesktop = 3;
        .c-list-link(role="list", data-length-mobile=lengthMobile, data-length-desktop=lengthDesktop)
            each set, index in list
                - var primaryItem = set.primary 
                .c-list__item(role="listitem", class=`${primaryItem ? "c-list__item--primary" : ""} ${index > lengthDesktop ? "hidden" : ""}`)
                    +listLinkArticleCard(set)
    
    mixin listLinkArticleCard({eyebrow, headline})
        +listLinkResetArticle({eyebrow, headline})

    //- mixin add
    mixin tableContent(t)
        if t == 1
            td 2016
        else if t == 2
            td OLED65E6, OLED65G6, OLED77G6
        else if t == 3
            td 2017
        else if t == 4
            td OLED65B7, OLED65C7, OLED65E7, OLED65G7, OLED65W7, OLED77G7, OLED77W7
        else if t == 5
            td 2018
        else if t == 6
            td Up to Sept 2019
        else 
            td(colspan='2') Search for your product above.


