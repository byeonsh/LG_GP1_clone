mixin CM0001(option=false, FirstLunching=false)
    mixin banner(params)
        - var option={color:'font-black',...params}
        // GA4
        a(href="#" class="c-list__link" data-link-name="" data-link-destination="" data-link-menu="" data-link-page-type="")
            // block : image
            // LGness 2023-03-21 : Modified Banner Image
            .c-image
                +printLazyImage('https://via.placeholder.com/620x316/999999/555555/?text=image')
            // LGness 2023-03-21 : Enter the color class for each banner
            .c-gnb__banner-floating(class=option.color)
                .c-text-contents
                    .title.c-text-contents__headline
                        .cmp-title(data-cmp-data-layer="")
                            strong.cmp-title__text=option.headline
                    if option.bodycopy
                        .text.c-text-contents__bodycopy
                            .cmp-text(data-cmp-data-layer="")
                                p=option.bodycopy
                .c-cta
                    if option.color == 'font-black'
                        +circleButton({name: 'Learn More', wSize: 'small', mSize: 'small', buttonStyle: 'arrow', isButton: "none"})
                    else
                        +circleButton({name: 'Learn More', wSize: 'small', mSize: 'small', buttonStyle: 'arrow', isButton: "none"})
    mixin cmpGroup(groupLv, otherClass)
        ul(class=`cmp-navigation__group c-gnb__group c-gnb__group--${groupLv}${otherClass ? ` ${otherClass}` : ``}`)
            if block
                block
    mixin cmpItem(cmpLv,itemLv, otherClass)
        li(class=`cmp-navigation__item cmp-navigation__item--level-${cmpLv} c-gnb__item c-gnb__item--${itemLv}${otherClass ? ` ${otherClass}` : ``}` data-cmp-data-layer="")
            if block
                block
    mixin group(groupLv, otherClass)
        ul(class=`c-gnb__group  c-gnb__group--${groupLv}${otherClass ? ` ${otherClass}` : ``}`)
            if block
                block
    mixin item(itemLv, otherClass)
        li(class=`c-gnb__item c-gnb__item--${itemLv}${otherClass ? ` ${otherClass}` : ``}`)
            if block
                block
    mixin cmpLinkHref(params)
        - var option={text:'', itemLv:'', otherClass:'', srOnly:false, target:'self', href:'#', ...params}
        - var attributes = {}
        - var roleButton = option.href != '#' ? attributes.role = 'button' : ''
        - var blank = option.target == 'blank' ? attributes.rel = 'noopener noreferrer' : ''
        if(option.href == '#CM0007')
            a(href=`${option.href}` class=`cmp-navigation__item-link c-gnb__item-link c-gnb__item-link${option.itemLv ? `--${option.itemLv}` : ``} ${option.otherClass && `${option.otherClass}`}` target=`_${option.target}` data-cmp-clickable)&attributes(attributes)
                if option.srOnly == true
                    span.sr-only #{option.text}
                    if block
                        block
                else
                    |#{option.text}
                    if block
                        block
        else
            // GA4
            a(href=`${option.href}` class=`cmp-navigation__item-link c-gnb__item-link c-gnb__item-link${option.itemLv ? `--${option.itemLv}` : ``} ${option.otherClass && `${option.otherClass}`}` target=`_${option.target}` data-cmp-clickable data-link-name="" data-link-destination="" data-link-menu="" data-link-page-type="")&attributes(attributes)
                if option.srOnly == true
                    span.sr-only #{option.text}
                    if block
                        block
                else
                    |#{option.text}
                    if block
                        block
    mixin cmpLinkBtn(params)
        - var option={text:'', itemLv:'', otherClass:'', srOnly:false, ...params}
        - var attributes = {}
        - var controls = option.controls ? attributes["aria-controls"] = option.controls : ''
        button(type="button", class=`cmp-navigation__item-link c-gnb__item-link c-gnb__item-link${option.itemLv ? `--${option.itemLv}` : ``} ${option.otherClass && `${option.otherClass}`}` data-cmp-clickable)&attributes(attributes)
            if option.srOnly == true
                span.sr-only #{option.text}
                if block
                    block
            else
                |#{option.text}
                if block
                    block
    mixin linkHref(params)
        - var option={text:'', itemLv:'', otherClass:'', srOnly:false, target:'self', ...params}
        - var attributes = {}
        - var blank = option.target == 'blank' ? attributes.rel = 'noopener noreferrer' : ''
        // GA4
        a(href="#" class=`c-gnb__item-link c-gnb__item-link${option.itemLv ? `--${option.itemLv}` : ``} ${option.otherClass && `${option.otherClass}`}` target=`_${option.target}` data-link-name="" data-link-destination="" data-link-menu="" data-link-page-type="")&attributes(attributes)
            if option.srOnly == true
                span.sr-only #{option.text}
                if(option.otherClass == 'icon-cart')
                    span.count 5
                if block
                    block
            else
                |#{option.text}
                if block
                    block
    mixin linkBtn(params)
        - var option={text:'', itemLv:'', otherClass:'', srOnly:false, ...params}
        - var attributes = {}
        - var controls = option.controls ? attributes["aria-controls"] = option.controls : ''
        if option.text == 'Search'
            a(href="#search", role="button", target="_self", class=`js-pop-open c-gnb__item-link c-gnb__item-link${option.itemLv ? `--${option.itemLv}` : ``} ${option.otherClass && `${option.otherClass}`}`)&attributes(attributes)
        else
            button(type="button", class=`c-gnb__item-link c-gnb__item-link${option.itemLv ? `--${option.itemLv}` : ``} ${option.otherClass && `${option.otherClass}`}`)&attributes(attributes)
                if option.srOnly == true
                    span.sr-only #{option.text}
                    if block
                        block
                else
                    |#{option.text}
                    if block
                        block
    mixin bcItem(params)
        - var option={text:'', current:false, ...params}
        li(class=`cmp-breadcrumb__item c-breadcrumb__item${option.current ? ` active` : ``}` data-cmp-data-layer="")
            if(option.current && option.heading)
                h1.cmp-breadcrumb__item-link.c-breadcrumb__item-link
                    span #{option.text}
            else if (option.current && !option.heading)
                strong.cmp-breadcrumb__item-link.c-breadcrumb__item-link
                    span #{option.text}
            else if (option.nolink)
                span.cmp-breadcrumb__item-link.c-breadcrumb__item-link
                    span #{option.text}
            else
                // GA4
                a.cmp-breadcrumb__item-link.c-breadcrumb__item-link(href="#" data-cmp-clickable="" data-link-name="" data-link-destination="" data-link-menu="" data-link-page-type="")
                    span #{option.text}
    mixin cookieItem(params)
        - var option={num:'',isDisabled:false, checked:false, title:'',desc:'',...params}
        .c-list__item(role="listitem" class=option.class)
            label.c-checkbox-toggle(for=`cookie-agree${option.num}` aria-labelledby=`cookie-agree-label${option.num}`)
                input(type="checkbox", name=option.class, value=`cookie-agree${option.num}`, id=`cookie-agree${option.num}` checked=option.checked disabled=option.isDisabled)
                span.label
                    span.sr-only #{option.title}
            .c-text-contents
                .title.c-text-contents__headline(id=`cookie-agree-label${option.num}`)
                    .cmp-title.font-w-normal-16.font-m-normal-12(data-cmp-data-layer="")
                        strong #{option.title}
                .text.c-text-contents__bodycopy.font-w-normal-12.font-m-normal-12
                    .cmp-text(data-cmp-data-layer="")
                        | #{option.desc}
    mixin cookieLayer(option=false)
