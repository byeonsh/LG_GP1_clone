                                //-     +formCheckbox("Show VIP Price Only", "FT00000007", "FV00000028", "FT00000007_FV00000028", 30)(aria-controls="resultTarget")
                    li
                        fieldset.c-filter-fieldset
                            legend.sr-only
                            .c-form__box
