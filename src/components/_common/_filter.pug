mixin filterPack(title, controlsId)
    fieldset.c-filter-fieldset
        legend.sr-only= title
        .c-accordion.js-accordion
            if(controlsId != 'filter-set-01')
                .c-accordion__box.js-accordion-handle-target(data-expand="independent")
                    .c-accordion__head
                        .font-w-normal-16.font-m-normal-16(id=`${controlsId}-label`)= title
                        .button
                            button.cmp-button.c-accordion__button.js-accordion-trigger(type="button" aria-expanded="false" aria-controls=`${controlsId}` data-independent="true" aria-labelledby=`${controlsId}-label`)
                                span.c-accordion__button-text.sr-only Learn More
                    .c-accordion__body(id=`${controlsId}` role='region')
                        if block
                            block
            else
                .c-accordion__box.js-accordion-handle-target.c-accordion__box--expand(data-expand="independent")
                    .c-accordion__head
                        .font-w-normal-16.font-m-normal-16= title
                        .button
                            button.cmp-button.c-accordion__button.js-accordion-trigger(type="button" aria-expanded="true" aria-controls=`${controlsId}` data-independent="true")
                                span.c-accordion__button-text.sr-only Learn More
                    .c-accordion__body(id=`${controlsId}` role='region')
                        if block
                            block

mixin filter(showDiscontinued=true)
    .c-filter-bar.can-sticky
        .button
            .c-filter__icon
            //- button.c-filter-bar__button(type="button" aria-expanded="false" aria-controls="filterArea")
                span.expand.sr-only Expand
                span.collapse.sr-only Collapse
            //- button.c-filter-bar__text-button(type="button" aria-expanded="false" aria-controls="filter-set-01") Sort by
            button.c-filter-bar__text-button(type="button" aria-expanded="false" aria-controls="filter-set-01") Type
            button.c-filter-bar__text-button(type="button" aria-expanded="false" aria-controls="filter-set-02") Capacity
            button.c-filter-bar__text-button(type="button" aria-expanded="false" aria-controls="filter-set-03") Color
            button.c-filter-bar__text-button(type="button" aria-expanded="false" aria-controls="filter-set-04") Price
            button.c-filter-bar__text-button(type="button" aria-expanded="false" aria-controls="filter-set-05") Discontinued Products

    .c-filter.productcollection__filters#filterArea
        .c-filter-title
            .c-text-contents
                .c-filter__icon
                .text.c-text-contents__headline
                    strong Filter
                    button.c-filter__close(type="button")
                        span.sr-only Close
        form.c-form.cmp-form
            .c-form__inner.cmp-options
                ul.f-list
                    li              
                        +filterPack("Type", "filter-set-01")
