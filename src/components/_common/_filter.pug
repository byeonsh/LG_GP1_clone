                                .c-form-box
                                    +formCheckbox("Bottom freezer", "FT00000002", "FV00000004", "FT00000002_FV00000004", 0)(aria-controls="resultTarget")
                                .c-form-box
                                    +formCheckbox("Side-by-side", "FT00000002", "FV00000005", "FT00000002_FV00000005", 4)(aria-controls="resultTarget")
                                .c-form-box
                                    +formCheckbox("Bottom freezer", "FT00000002", "FV00000006", "FT00000002_FV00000006", 6)(aria-controls="resultTarget")
                                .c-form-box
                                    +formCheckbox("Kimchi/Speciality Foods", "FT00000002", "FV00000007", "FT00000002_FV00000007", 4)(aria-controls="resultTarget")
                                .c-form-box
                                    +formCheckbox("Freezer", "FT00000002", "FV00000008", "FT00000002_FV00000008", 10)(aria-controls="resultTarget")
                                .c-form-box
                                    +formCheckbox("Bottom freezer", "FT00000002", "FV00000009", "FT00000002_FV00000009", 15)(aria-controls="resultTarget")
                                .c-form-box
                                    +formCheckbox("French door", "FT00000002", "FV00000010", "FT00000002_FV00000010", 12)(aria-controls="resultTarget")
                    li
                        +filterPack("Capacity", "filter-set-02")
