                            const dragbarVal = {
                                priceOption:[
                                    {'Min':''},
                                    {'£500':'RG00044272'},
                                    {'£1200':'RG00044273'},
                                    {'£2000':'RG00044274'},
                                    {'£3000':'RG00044275'},
                                    {'Max':''}
                                ]
                            }
                            if(dragbarVal.priceOption){
                                dragbarVal.priceOption = JSON.parse(JSON.stringify(dragbarVal.priceOption).replace(/\&nbsp;/g, " "));
                            }                    
                        +filterPack("Price", "filter-set-04")
                            .c-form__box
                                +formRange("Price", "priceOption", "FT00000005", "FT00000006", "FT00000005", "FT00000006", 0, 5, "Minimum price for filter", "Maximum price for filter")(aria-controls="resultTarget")
