                                    if data[0].discount
                                        +price-info({unitPos:data[0].unitPosition, unitName:data[0].unitName, emphasis : true, useTooltip : false, bold : true, name : "Discount", class:"c-price-info__discount", price : "18.00", detail: [{"Welcome Coupon" : "18.00"}, {"Applied points" : "18.00"}]})
