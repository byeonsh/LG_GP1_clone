mixin cartPriceTotal(data, options)
    // block : product price sticky
    .cart-price-total
        .c-product-price-sticky(class={'js-sticky':data[0].isStickyBottom, 'c-product-price-sticky--m-bottom-fixed': data[0].isMobileBottomFixed, 'c-product-price-sticky--fold-toggle':data[0].useToggleFoldBtn})
            if data[0].msg
                +toastPopup(data[0].msg, 'checkout-aria')
            // block : product price information
            //- RO fullcase active hidden 
            //- .c-product-price-information.web.active(class=`${data[0].empty ? 'c-product-price-information--empty' : ''}`)
            .c-product-price-information.web(class=`${data[0].empty ? 'c-product-price-information--empty' : ''}` class=`${data[0].close ? 'close' : ''}`)
                .c-product-price-information__inner
                    .button.c-product-price-information__btn-toggle
                        button.cmp-button(aria-controls="productPriceDetailInformation" aria-expanded="false" data-cmp-data-layer="")
                            span.cmp-button__text.sr-only open/close
                    // block : price box
                    +total-price(data[0])
                    .buy-total__container(class=`${data[0].discount ? 'buy-total__container--discount-sapce':''}`)
                        if data[0].coupon
                            .c-text-contents__bodycopy.text-coupon
                                +printLazyImage(`../../assets/icon/icon-coupon-24-24.svg`)
                                span.font-w-normal-14.font-m-normal-12 You have 
                                    span.font-red 3 coupons
                                    | . Apply at checkout.
                        .text.c-price-box.font-w-normal-24.font-m-normal-16(id="productPriceDetailInformation")
                            if !data[0].empty
                                .c-price-box__item.c-price-box__item--list
                                    if data[0].totalDetail
                                        each depth1 in data[0].totalDetail
                                            +price-info({
                                                unitPos:data[0].unitPosition,
                                                emphasis : false,
                                                useTooltip : false,
                                                bold : true,
                                                name : depth1.parent_title,
                                                price : depth1.parent_price,
                                                detail: depth1.children,
                                                mixinType : "text"
                                            })
                                    if data[0].taxIncludeSG
                                        .c-price-info.c-price-info__subject.font-semibold
                                            .c-price-info__title.cmp-text(data-cmp-data-layer="")
                                                p Product Price before discount 
                                                    br 
                                                    | (incl 9% GST)
                                            .c-price-info__price
                                                p $N,NNN.NN
                                        .c-price-info.c-price-info__subject
                                            .c-price-info__title.cmp-text(data-cmp-data-layer="")
                                                p Total Tax after discount 
                                                    br 
                                                    | (9% GST)
                                            .c-price-info__price
                                                p $NNN.NN
                                    //- [Start] 2024-05-07 ADD : [PH] BUY payment - taxIncludePH
                                    if data[0].taxIncludePH
                                        .c-price-info.c-price-info__subject.font-semibold
                                            .c-price-info__title.cmp-text(data-cmp-data-layer="")
                                                p Product price before discount
                                                    br 
                                                    | (with 12% VAT)
                                            .c-price-info__price
                                                p ₱ 2,099.98
                                        .c-price-info-detail
                                            .c-price-info-detail__name.cmp-name.font-w-normal-16.font-m-normal-12(data-cmp-data-layer="")
                                                span OLW480B
                                            .c-price-info-detail__price.cmp-price.font-w-semibold-16.font-m-semibold-14(data-cmp-data-layer="")
                                                span ₱ 2,959.00
                                    //- [End] 2024-05-07 ADD : payment PH - taxIncludePH
                                    //- [Start] priceListCustomize
                                    if data[0].priceListCustomize
                                        each item in data[0].priceListCustomize 
                                            -const detailSubList = []
                                            -let sebUseTooltipTitle = [];
                                            if item.sub
                                                each subItem in item.sub
                                                    -sebUseTooltipTitle.push(subItem.isTooltip ? subItem.title : '');
                                                    -let obj = {};
                                                    -obj[subItem.title] = subItem.price;
                                                    -detailSubList.push(obj)
                                            +price-info({
                                                unitPos:data[0].unitPosition,
                                                unitName:item.unitName?item.unitName :data[0].unitName,
                                                emphasis : item.emphasis,
                                                useTooltip : item.isTooltip,
                                                bold : true,
                                                name : item.title,
                                                price : item.price,
                                                sebUseTooltip : sebUseTooltipTitle,
                                                detail: detailSubList
                                            })
                                    //- [End] priceListCustomize
                                    if data[0].taxInclude
                                        - let taxIncludeValue = typeof data[0].taxInclude === "boolean" ?  "Subtotal (Tax included)" : data[0].taxInclude
                                        +price-info({emphasis : false, useTooltip : false, bold : true, name : taxIncludeValue, price : "£4,199.96"})
                                    if data[0].taxIncludeSub
                                        +price-info({unitPos:data[0].unitPosition, unitName:data[0].unitName, emphasis : false, useTooltip : false, bold : true, name : "Subtotal (税込)", price : "146,990"})
