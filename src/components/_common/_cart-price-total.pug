mixin unitPos(pos, price, unitName)
    // [Start] 2023-05-31 : UPDATE -about product price
    if pos == 'right' 
        = `${price}${unitName ? unitName : '€'}` 
    else if pos == 'sup'
        = `${price}`
        sup.c-price__unit=`${unitName ? unitName : '₫'}`
    else if pos == 'cn'
        span.c-price__smallFont=`${unitName ? unitName : ' ￥'}`
        = `${price}`
        span.c-price__smallFont .00
    else
        = `${unitName ? unitName : '£'}${price}`
    // [End] 2023-05-31 : UPDATE -about product price


mixin price-info(params)
    - var option = {list:false, dimmed:false, bold:false, ehf:false, ...params}

    // block : price info
    .c-price-info.c-price-info__subject(class={'font-red' : option.emphasis, 'sub-tree': option.list, 'font-semibold' : option.bold,'dimmed': option.dimmed, 'EarnedPoint': option.point} class=`${option.class ? option.class : ''}`)
        .c-price-info__title.cmp-text(data-cmp-data-layer="")
            if(option.link)
                a(href="#" data-link-name="" data-link-destination="" data-link-menu="" data-link-page-type="")= option.name
            else
                p= option.name
