        h2 Form
        section.c-form(style="padding-top: 30px; max-width: 800px; margin: 0 auto;")
            h3 Input text 
            div(style="padding:20px; background: #f6f6f6; border-radius: 8px; margin: 5px 0 10px;")
                +formText("text", "Email Address", "test1", true, "Please input your email")
                +formText("text", "Email Address", "test2", true, "Please input your email")(disabled="disabled")
            h3 Select box
            div(style="padding:20px; background: #f6f6f6; border-radius: 8px; margin: 5px 0 10px;")
                +formSelect("Country", "test3", "Select Country", false)
                    option(value="") Select Country
                    option(value="country1") Country1
                    option(value="country2") Country2
                    option(value="country3") Country3
                +formSelect("Country", "test4", "Select Country", false)(disabled="disabled")
                    option(value="") Select Country
                    option(value="country1") Country1
                    option(value="country2") Country2
                    option(value="country3") Country3
            h3 Checkbox
            div(style="padding:20px; background: #f6f6f6; border-radius: 8px; margin: 5px 0 10px; display: flex; gap: 10px; flex-wrap: wrap;")
                +formCheckbox("label", "name", "val1", "checkId1", count="false", false)
                +formCheckbox("has count", "name", "val2", "checkId2", 3, false)(checked)
                +formCheckbox("checked disabled", "name", "val4", "checkId4", count="false", true)(checked)
                +formCheckbox("disabled", "name", "val3", "checkId3", count="false", true)
            h3 Radio
            div(style="padding:20px; background: #f6f6f6; border-radius: 8px; margin: 5px 0 10px; display: flex; gap: 10px; flex-wrap: wrap;")
                h4(style="flex-basis:100%;") default
                +formRadio("checked", "name1", "val1a", "radioId1a")(checked)
                +formRadio("default", "name1", "val1b", "radioId1b")
                +formRadio("disabled", "name1", "val1c", "radioId1c")(disabled="disabled")
                h4(style="flex-basis:100%;") check-shaped
                +formRadio("checked", "name2", "val2a", "radioId2a", "check-shaped")(checked)
                +formRadio("default", "name2", "val2b", "radioId2b", "check-shaped")
                +formRadio("disabled", "name2", "val2c", "radioId2c", "check-shaped")(disabled="disabled")
                h4(style="flex-basis:100%;") has arrow
                    div(style="flex-basis:100%;")
                        +formRadio("checked", "name3", "val3a", "radioId3a", "has-next")(checked)
                        +formRadio("default", "name3", "val3b", "radioId3b", "has-next")
                        +formRadio("disabled", "name3", "val3c", "radioId3c", "has-next")(disabled="disabled")
                h4(style="flex-basis:100%;") block type
                    div(style="flex-basis:100%;")
                        +formRadio("checked", "name4", "val4a", "radioId4a", "is-block")(checked)
                        +formRadio("default", "name4", "val4b", "radioId4b", "is-block")
                        +formRadio("disabled", "name4", "val4c", "radioId4c", "is-block")(disabled="disabled")
            h3 Swatch
            div(style="padding:20px; background: #f6f6f6; border-radius: 8px; margin: 5px 0 10px; display: flex; gap: 10px; flex-wrap: wrap;")
                .c-form
                    .c-form-box
                        +formSwatch("White", "FT00000004", "FV00000015", "FT00000004_FV00000015", "border:1px solid #e6e6e6;background-color: #fff;")
                        +formSwatch("Iceblue", "FT00000004", "FV00000016", "FT00000004_FV00000016", "background: linear-gradient(180deg, #a7a3a2 0%, rgba(178, 175, 174, 0.08) 50%, #b2afae 100%)")
                        +formSwatch("Silver", "FT00000004", "FV00000017", "FT00000004_FV00000017", "background-color: #a4a4a4;")
                        +formSwatch("Black", "FT00000004", "FV00000018", "FT00000004_FV00000018", "background-color: #000;")
                        +formSwatch("Dark Gray", "FT00000004", "FV00000019", "FT00000004_FV00000019", "background: linear-gradient(180deg,rgba(106, 105, 105, 0.92) 0%,rgba(76, 76, 76, 0.37) 46.88%,rgba(30, 30, 30, 0.58) 100%);")
            h3 Range
            div(style="padding:20px; background: #f6f6f6; border-radius: 8px; margin: 5px 0 10px; display: flex; gap: 10px; flex-wrap: wrap;")
                .c-form
                    script. 
                        const dragbarVal = {
                            priceOption:[
                                {'Min':''}, 
                                {'£500 - 1199':'RG00044272'},
                                {'£1200 - 1999':'RG00044273'},
                                {'£2000 - 2999':'RG00044274'},
                                {'£3000<':'RG00044275'},
                                {'Max':''}
                            ]
                        }
                        if(dragbarVal.priceOption){
                            dragbarVal.priceOption = JSON.parse(JSON.stringify(dragbarVal.priceOption).replace(/\&nbsp;/g, " "));
                        }
                    .c-form__box(style="width:300px")
                        +formRange("Price", "priceOption", "FT00000005", "FT00000006", "FT00000005", "FT00000006", 0, 5)
            h3 exclamTooltip
            div(style="padding:20px; background: #f6f6f6; border-radius: 8px; margin: 5px 0 10px; display: flex; gap: 10px; flex-wrap: wrap;")
                +exclamTooltip
