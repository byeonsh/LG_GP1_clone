                            ul.c-tabs__tablist.cmp-tabs__tablist.swiper-wrapper.CS0304(role="tablist", aria-multiselectable="false")
                                +printTab(1, 1, true)
                                    span 总则
                                +printTab(1, 2, false)
                                    span 安装收费基准
                                +printTab(1, 3, false)
                                    span 维修收费基准

                        +printTabPanel(1, 1, true)
                            .container.CS0304__tabcon
