                brandDistance: '2.2 KM',
                brandName: 'Paulson’s',
                brandAddr: {address: '1880 Broadway New York, NY 10023, United States', hasExport: false},
                contact: [
                    {type: 'phone', number: ['212-246-9734']},
                ],
                buttons: [
