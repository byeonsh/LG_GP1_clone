extends ../_common/_layout.pug
include ../_common/_button.pug
include ../_common/_image.pug
include ../_common/_forms.pug
include ../_common/_pop-msg.pug
include ../_common/_toast.pug

block meta 
    - var componentId = "CT000F"
    - var componentName = "Checkout Popup"
    - var description = "description text"
    - var keywords = "keyword list"
    title #{componentId}. #{componentName}
    meta(name="description" content=description)
    meta(name="keywords" content=keywords)

block styles
    link(rel="stylesheet", href=componentId+".css")

block scripts
    script(src=componentId+".js" type="module")
mixin myMsg({infoText= false, correct= false, warning= false, check= false, warningFirst, infoTextSecond})
    if warningFirst
        - for(var i=0; i < warningFirst.length; i++)
            +invalid-warning(warningFirst[i])
    if correct
        - for(var i=0; i < correct.length; i++)
            +valid-msg(correct[i])
    if infoText
        ul.my-input__info 
            li.font-w-normal-14.font-m-normal-14!= infoText
    if warning !==  []
        - for(var i=0; i < warning.length; i++)
            +invalid-warning(warning[i])
    if check
        - for(var i=0; i < check.length; i++)
            +formCheckbox(check[i], "name", "val1", `checkId${i}`, count="false", false)
    if infoTextSecond
        ul.my-input__info 
            each infoText in infoTextSecond
                li.my-input__info__second.font-w-normal-16.font-m-normal-14= infoText

mixin invalid-warning(text = "warning msg")
    .c-warning.font-m-normal-14.font-w-normal-14 !{text}

mixin myFormText(params)
    - let data = {...params}
    - let config = `${data.warning || data.warningFirst && !data.correct ? 'my-warn' : ''}`
        if data.type === 'select'
            +mySelect({optionVal: data.optionVal, warning: data.warning, label: data.label, id: data.id, infoText: data.infoText, placeholder: data.placeholder, warn: data.warn, selected: data.selected, tooltipMsg:data.tooltipMsg, readonly:data.readonly, disabled:data.disabled, infoTextSecond: data.infoTextSecond})
        else if data.type === 'selectMulti'
            .c-select-item
                label.font-w-normal-16.font-m-normal-14(for= data.id)= data.label
                .c-select-item--selectMulti
                    each option, idx in data.optionVal
                        select(id=`${data.id}-${idx}`, title=`${data.title}`)&attributes(attributes)
                            - for(var i=0; i < option.length; i++)
                                if selected === i + 1
                                    option(value="" selected)= option[i]
                                else
                                    option(value="")= option[i]
                if message
                    +postFormMessage(`${message}`, "")
            +myMsg({infoText: data.infoText, infoTextSecond: data.infoTextSecond, correct: data.correct, warning: data.warning, check: data.check, warningFirst: data.warningFirst})
        else if data.type === 'searchAddress'
            .c-input-item.searchAddress(class=data.addressClass)
                label.font-w-normal-20.font-m-normal-16(for= data.id)= data.label
                    if data.tooltipMsg
                        +exclamTooltip(data.tooltipMsg, false)
                .searchAddress__inr
                    .c-input-item
                        if data.required
                            input(type= data.type id= data.id value= data.value placeholder= data.placeholder data-placeholder=data.placeholder class=config required)&attributes(attributes)
                        else
                            input(type= data.type id= data.id value= data.value placeholder= data.placeholder data-placeholder=data.placeholder class=config)&attributes(attributes)
                    if data.list
                        .searchAddress__list 
                            each item in data.list
                                button(type="button" class="searchAddress__item")= item
                    if data.clearCountry != true 
                        .searchAddress__country
                            p.searchAddress__country-textcontent
                                img(src=`../../assets/icon/${data.countryImg}` alt=`${data.countryName}`).searchAddress__country-image
                                span.searchAddress__country-text.font-w-normal-16.font-m-normal-16= data.countryName
                            if data.list
                                button(type="button").searchAddress__country-button
                                    span.sr-only Change Country
            if data.doubleInputNo > 0
              +myMsg({correct: data.correct, warning: data.warning, check: data.check, warningFirst: data.warningFirst})
            else
              +myMsg({correct: data.correct, infoTextSecond: data.infoTextSecond, warning: data.warning, check: data.check, warningFirst: data.warningFirst})
        else if data.type === 'searchAddressSelect'
            .c-input-item 
                label.font-w-normal-20.font-m-normal-16(for= data.id)= data.label
                details.searchAddressSelect(class=data.addressClass)
                    summary.searchAddress__title= data.title
                    .searchAddress__inr
                        .c-input-item
                            input(type= data.type id= data.id value= data.value placeholder= data.placeholder data-placeholder=data.placeholder class=config)&attributes(attributes)
                        if data.list
                            .searchAddress__list 
                                each item in data.list
                                    button(type="button" class="searchAddress__item")= item
                if data.required
                    +myMsg({correct: data.correct, warning: ['Este é um campo obrigatório.'], check: data.check, warningFirst: data.warningFirst})                                
                +myMsg({correct: data.correct, warning: data.warning, check: data.check, warningFirst: data.warningFirst, infoTextSecond: data.infoTextSecond})
        else if data.type === 'checkbox'
            .c-input-item
                if data.label
                    label.font-w-normal-16.font-m-normal-14(for= data.id class=`${data.info ? 'my-info' : ''}`)= data.label
                +formCheckbox(data.text, "name", "val1", `checkId-${data.id}`, count="false", false, false, false, data.checked)
        else if data.type === 'tel'
            .c-input-item(class=`${data.telNumber ? 'c-input-item--tel':''}`)
                label.font-w-normal-16.font-m-normal-14(for= data.id class=`${data.info ? 'my-info' : ''}`)= data.label
                if data.tooltipMsg
                    if !data.tooltipInInput
                        +exclamTooltip(data.tooltipMsg, false)
                if data.tooltipInInput
                    if data.numberText
                        .number-text.font-w-normal-14.font-m-normal-12= data.numberText
                    .c-tooltip__input
                        input(type='tel' id= data.id placeholder= data.placeholder data-placeholder= data.placeholder maxlength=data.maxlength class=`${config}` value= data.value)&attributes(attributes)
                        if data.telNumber
                            .my-form-number.font-w-normal-16.font-m-normal-16=data.telNumber
                        +exclamTooltip(data.tooltipMsg, false, 'bottom-right')
                else
                    if data.numberText
                        .number-text.font-w-normal-14.font-m-normal-12= data.numberText
                        if data.telNumber
                            .c-input-item
                                input(type='tel' id= data.id placeholder= data.placeholder data-placeholder= data.placeholder maxlength=data.maxlength class=`${config}` value= data.value)&attributes(attributes)
                                .my-form-number.font-w-normal-16.font-m-normal-16=data.telNumber
                        else 
                            input(type='tel' id= data.id placeholder= data.placeholder data-placeholder= data.placeholder maxlength=data.maxlength class=`${config}` value= data.value)&attributes(attributes)
                    else if data.telNumber
                        .c-input-item
                            input(type='tel' id= data.id placeholder= data.placeholder data-placeholder= data.placeholder maxlength=data.maxlength class=`${config}` value= data.value)&attributes(attributes)
                            .my-form-number.font-w-normal-16.font-m-normal-16=data.telNumber
                    else 
                        input(type='tel' id= data.id placeholder= data.placeholder data-placeholder= data.placeholder maxlength=data.maxlength class=`${config}` value= data.value)&attributes(attributes)
            +myMsg({infoText: data.infoText, infoTextSecond: data.infoTextSecond, correct: data.correct, warning: data.warning, check: data.check, warningFirst: data.warningFirst})       
            if data.telInfo 
                    .telInfo.font-w-normal-16.font-m-normal-14=data.telInfo
        else if data.type ===  'searchAddressGoogle'
            .c-input-item
                label.font-w-normal-16.font-m-normal-14(for= data.id)= data.label
                    if data.required
                        em.c-required *
                            span.sr-only Required fields
                .c-input-item
                    input(type='text' id= data.id value= data.value placeholder= data.placeholder data-placeholder=data.placeholder class=`${config}`)&attributes(attributes)
            +myMsg({correct: data.correct, warning: data.warning, check: data.check, warningFirst: data.warningFirst})
            .c-input-item__address-info
                .c-input-item__address-info__text 
                    span.font-w-normal-16.font-m-normal-14 Eg. 12 Ridge Street
                .c-input-item__address-info__copy
                    .c-input-item__address-info__text 
                        span.font-w-normal-16.font-m-normal-16 powered by
                    span.c-input-item__address-info__copy__image
        else
            .c-input-item(class=`${data.active ? 'active' : ''}${data.frontText?'c-input-item--tax':''}`)
                if data.label
                    label.font-w-normal-16.font-m-normal-14(for= data.id class=`${data.info ? 'my-info' : ''}`)= data.label
                    if data.tooltipMsg
                        +exclamTooltip(data.tooltipMsg, false)
                        // [Start] 2023-06-12 : ADD - number-text
                        if data.numberText
                            .number-text.font-w-normal-14.font-m-normal-12= data.numberText
                        // [End] 2023-06-12 : ADD - number-text
                if data.required
                    input(type= data.type id= data.id placeholder= data.placeholder data-placeholder=data.placeholder class=config value=data.value, required)&attributes(attributes)
                else if data.readonly
                    input(type= data.type id= data.id placeholder= data.placeholder data-placeholder=data.placeholder class=config value=data.value, readonly)&attributes(attributes)
                else if data.disabled
                    input(type= data.type id= data.id placeholder= data.placeholder data-placeholder=data.placeholder class=config value=data.value, disabled)&attributes(attributes)
                else if data.button
                    if data.validation
                        .input-button__container.input-button__container--validation(class= `${data.buttonDirectionRow ? 'input-button__container--direction-row' : ''}`)
                            .my-button-validation
                                input(type= data.type id= data.id, placeholder= data.placeholder data-placeholder= data.placeholder class=config value=data.value)&attributes(attributes)
                                +myMsg({correct: data.correct, warning: data.warning, check: data.check, warningFirst: data.warningFirst})
                            .button.c-cta.button-full
                                if data.button === 'Find Address'
                                    +defaultButton({name:data.button, wSize: 'medium', mSize: 'medium', buttonStyle: `${data.buttonStyle ? data.buttonStyle : 'default'}`})(role="search")
                                else if data.button === 'Check'
                                    +defaultButton({name:data.button, wSize: 'medium', mSize: 'medium', buttonStyle: 'highlight'})
                                else 
                                    +defaultButton({name:data.button, wSize: 'medium', mSize: 'medium', buttonStyle: `${data.buttonStyle ? data.buttonStyle : 'default'}`})
                    else
                        .input-button__container(class= `${data.buttonDirectionRow ? 'input-button__container--direction-row' : ''}`)
                            input(type= data.type id= data.id, placeholder= data.placeholder data-placeholder= data.placeholder class=config value=data.value)&attributes(attributes)
                            .button.c-cta.button-full
                                if data.button === 'Find Address'
                                    +defaultButton({name:data.button, wSize: 'medium', mSize: 'medium', buttonStyle: `${data.buttonStyle ? data.buttonStyle : 'default'}`})(role="search")
                                else if data.button === 'Check'
                                    +defaultButton({name:data.button, wSize: 'medium', mSize: 'medium', buttonStyle: 'highlight'})
                                else 
                                    +defaultButton({name:data.button, wSize: 'medium', mSize: 'medium', buttonStyle: `${data.buttonStyle ? data.buttonStyle : 'default'}`})
                else if data.frontText
                    .c-input-item
                        input(type='text' id= data.id placeholder= data.placeholder data-placeholder= data.placeholder maxlength=data.maxlength class=`my-input__password ${config} ${data.class}` value= data.value)&attributes(attributes)
                        .my-form-number.font-w-normal-16.font-m-normal-16=data.frontText
                else 
                    input(type='text' id= data.id placeholder= data.placeholder data-placeholder= data.placeholder maxlength=data.maxlength class=`my-input__password ${config} ${data.class}` value= data.value)&attributes(attributes)
                if data.tag
                    | !{data.tag}
            +myMsg({infoText: data.infoText, infoTextSecond: data.infoTextSecond, correct: data.correct, warning: data.warning, check: data.check, warningFirst: data.warningFirst})
        if data.doubleInputNo > 0
            - for (let i=1; i<=data.doubleInputNo; i++)
                .c-input-item
                    input(type='text' id=`${data.id}-${i}` class=`my-input__password`)&attributes(attributes)
            if data.doubleInputWarning 
                +myMsg({warning: data.doubleInputWarning, warningFirst: data.doubleInputWarningFirst})
            if data.type === 'searchAddress'
                +myMsg({infoTextSecond: data.infoTextSecond})

mixin mySelect({optionVal=false, warning=false, label='', id='', infoText= false, placeholder='', warn=false, selected, tooltipMsg=false, readonly=false, disabled=false, infoTextSecond=false})
    .c-search-box__form(class=`${warning ? 'my-warn' : ''}`)
        +formSelect(label, id, placeholder, false,false,false,tooltipMsg, readonly, disabled)
            if optionVal
                - for(var i=0; i < optionVal.length; i++)
                    if selected === i + 1
                        option(value="" selected)= optionVal[i]
                    else
                        option(value="")= optionVal[i]
        if warning || infoText || infoTextSecond
            +myMsg({warning: warning, infoText: infoText, infoTextSecond: infoTextSecond,})

mixin formSelect(label, id, title, required=false, labelSrOnly=false, message = false, tooltipMsg=false, readonly=false, disabled=false)
    .c-select-item
        if label != ''
            - var labelStyle = "font-w-normal-16 font-m-normal-14"

            label(for=`${id}` class=labelStyle class={'sr-only' : labelSrOnly})= label
                if(required) 
                    +required-text-sr
                
            if tooltipMsg
                if !tooltipInInput
                    +exclamTooltip(tooltipMsg, false)
        //- Bad value true for attribute required on element input.
        if required
            select(id=`${id}`, title=`${title}`, required="required")&attributes(attributes)
                if block
                    block
        else if readonly
            select(id=`${id}`, title=`${title}`, readonly="readonly")&attributes(attributes)
                if block
                    block
        else if disabled
            select(id=`${id}`, title=`${title}`, disabled="disabled")&attributes(attributes)
                if block
                    block
        else
            select(id=`${id}`, title=`${title}`)&attributes(attributes)
                if block
                    block
        if message
            +postFormMessage(`${message}`, "")

mixin myInputForm(data)
    .my-input
        ul
            each set in data
                li(class=`${!set.label ? 'empty-label':''}`)
                    +myFormText(set)

//- new mixin
mixin buyAnotherAddressRadio(label, name, val, id, address, edit = true)
    .c-radio-item.buy-coupon-item
        label.radio(for=`${id}`)
            input(type="radio" name=`${name}`, value=`${val}`, id=`${id}`)&attributes(attributes)
            span.label
                span.c-text-contents
                    span.text.c-text-contents__bodycopy
                        span.cmp-text.font-w-normal-16.font-m-normal-16.buy-another-address__label(data-cmp-data-layer="")= label
                        span.cmp-text.font-w-normal-20.font-m-normal-16.buy-another-address__address(data-cmp-data-layer="")= address
        if edit
            .c-radio-item--edit.button
                +textButton({name: 'Edit', underline:true, wSize: 'small', mSize: 'small', buttonStyle: 'default', isButton: true})

mixin buyAnotherAddress(title, params)
    - let data = {...params}
    .buy-another-address
        .title.c-text-contents__title
            if title === 'Shipping Address'
                span.cmp-title.font-w-normal-20.font-m-normal-16.buy-another-address__title.buy-another-address__title--ship(data-cmp-data-layer="")= title
            if title === 'Billing Address'
                span.cmp-title.font-w-normal-20.font-m-normal-16.buy-another-address__title.buy-another-address__title--bill(data-cmp-data-layer="")= title
        each set in data
            +buyAnotherAddressRadio(set.addressTitle, set.name, set.val, set.id, set.address, set.edit)

mixin exclamTooltip(tooltipText, usedButton = true, direction = 'bottom-left', size = '')
    - var text = tooltipText || "Lorem ipsum  Neque porro quisquam est qui dolorem ipsum quia dolor sit amet, consectetur, adipisci velit.."
    if (size == 'large')
        - var size = 'c-tooltip__contents--large'
    if (size == 'medium')
        - var size = 'c-tooltip__contents--medium'
    span.c-tooltip
        span.button
            button.cmp-button.c-icon-button.c-icon-button--more-info.c-tooltip__button(type="button" aria-expanded="false", data-cmp-data-layer="")
                span.sr-only More information
        span.c-tooltip__container(class=direction)
            span.c-tooltip__contents.has-close(class=size)
                button.c-tooltip__close(type='button')
                    span.sr-only Close
                span.c-text-contents
                    span.text.c-text-contents__bodycopy
                        span.cmp-text.font-w-normal-14.font-m-normal-14(data-cmp-data-layer="")
                            span= text
                    if usedButton
                        span.button
                            +linkButton({type:"text", color:"black", icon: true, name: "Learn More"})

mixin bankDetailList(data)
    .bank-details__list
        each set in data
            dl.bank-details__item
                dt.bank-details__title= `${set.title} :`
                dd.bank-details__text= set.details

mixin checkoutStep(params)
    case params.type
        when "active"
            .c-checkout-step(class=params.type)
                +checkoutStepItem(params)
        when "checked"
            .c-checkout-step(class=params.type)
                +checkoutStepItem(params)
        when "before"
            .c-checkout-step(class=params.type)
                +checkoutStepItem(params)
        when "complete"
            //- [Start] 2023-10-11 : UPDATE - added complete case, fixed text
            .c-checkout-step(class=params.type)
                h4.c-checkout-step__title= params.title
            //- [Edn] 2023-10-11 : UPDATE - added complete case, fixed text
        default
            .c-checkout-step
                +checkoutStepItem(params)

mixin checkoutStepItem(params)
    h4.c-checkout-step__title= params.title
    if params.type === 'checked'
        +button({type:"text", color:"black", size:"small", underline: true, name: "Edit"})(class="c-checkout-step__edit")
    else 
        div.c-checkout-step__box
            span.c-checkout-step__txt Шаг
            span.c-checkout-step__current= params.current
            span.c-checkout-step__total= `/ ${params.total}`

mixin radioList(data)
    .installment-radio__list
        each set in data
            if set.disabled
                input(type="radio" id=`install-${set.val}` name=`install-${set.name}` disabled class="installment-radio__input disabled")
            else
                input(type="radio" id=`install-${set.val}` name=`install-${set.name}` class="installment-radio__input")
            label(for=`install-${set.val}` class="installment-radio__item")
                span.installment-radio__head
                    span.installment-radio__text= set.text
                        if set.paymentImg
                            img(src=`../../assets/icon/${set.paymentImg}` alt=`${set.paymentName}`).installment-radio__payment-image
                    span.installment-radio__badge= set.badge

mixin infoList(data)
    .installment-pop-item__info
        //- [Start] 2023-10-11 : UPDATE - added complete case, fixed text
        if data.title
            p.installment-pop-item__info__title= data.title
        //- [End] 2023-10-11 : UPDATE - added complete case, fixed text
        ul.installment-pop-item__list
            each set in data.lists
                li
                    span.installment-pop-item__text= set.text
                        if set.paymentImg
                            img(src=`../../assets/icon/${set.paymentImg}` alt=`${set.paymentName}`).installment-pop-item__payment-image
                    if set.badge
                        span.installment-pop-item__badge= set.badge
                    else
                        span.installment-pop-item__value= set.value


mixin formCheckbox(label, name, val, id, count = "false", disabled = false, arialabelledby = false, labelledby)
    .c-checkbox-item&attributes(attributes)
        label.checkbox(for=`${id}`)
            if(arialabelledby)
                if(disabled)
                    input(type="checkbox", name=`${name}`, value=`${val}`, id=`${id}`, disabled="disabled" aria-labelledby=`${labelledby}`)&attributes(attributes)
                else 
                    input(type="checkbox", name=`${name}`, value=`${val}`, id=`${id}`  aria-labelledby=`${labelledby}`)&attributes(attributes)
            else
                if(disabled)
                    input(type="checkbox", name=`${name}`, value=`${val}`, id=`${id}`, disabled="disabled")&attributes(attributes)
                else 
                    input(type="checkbox", name=`${name}`, value=`${val}`, id=`${id}`)&attributes(attributes)

            if(label)
                span.label !{label}
                    -if(count >= 0 & count != "false") // for filter
                        sup.count= count 
            else 
                if block 
                    span.label
                        block

mixin checkboxSelect(label, name, val, id)
    input(type="checkbox", name=`${name}`, value=`${val}`, id=`${id}`)&attributes(attributes)
    label.checkbox(for=`${id}`)
        span.label= label

mixin formRadioItem(label, name, val, id, check=false)
    input(type="radio" name=`${name}`, value=`${val}`, id=`${id}`)&attributes(attributes)
    label.radio(for=`${id}`)
        span.label= label

mixin pop-msg-standard(params)
    +pop-msg-layout(params)
        block

mixin pop-msg-layout(params)
    - var option={id:`popMsg${new Date().getTime()}`, ...params}
    - var dialogId = option.id
    - var dialogLabelId = `${option.id}-headline`
    - var dialogSize = option.size || "medium"
    - var dialogClass = `${dialogSize}`
    - var useHeadline = option.headline != null && 0 < option.headline.length
    // block - pop-msg ( dialog )
    .c-pop-msg(class=`${dialogClass}` class={[option.classes] : !!option.classes} id=`${dialogId}` role="dialog" aria-modal="true" aria-labelledby=useHeadline&&dialogLabelId)
        .c-pop-msg__container
            if useHeadline
                //- header
                +pop-msg-header(params)
            //- contents
            if block
                block
            //- close button
            if(!option.btnCloseNone)
                .button.c-pop-msg__close.c-pop-msg__close--on-top
                    button.cmp-button.c-pop-msg__close-button.js-pop-close(type="button", aria-controls=`${dialogId}` data-cmp-data-layer="")
                        span.cmp-button__text.sr-only close
        .c-pop-msg__dimmed

mixin pop-msg-header(params)
    - var option = {headerBorder : true, headerSrOnly: false, align : "left",  ...params}
    - var dialogHeadline = option.headline || "Dialog Headline"
    - var dialogDescription = option.headerDescription || "Dialog Description"
    - var headerBorder = option.headerBorder ? '' : 'c-pop-msg__header--no-border'
    - var headerMargin = option.headerMargin ? `c-pop-msg__header--margin-${option.headerMargin}` : ''
    - var dialogLabelId = `${option.id}-headline`
    - var headerSrOnly = option.headerSrOnly ? 'sr-only' : ''
    - var alignClass = option.align
    .c-pop-msg__header(class=`${headerBorder} ${headerMargin} ${headerSrOnly}` class={[alignClass] : 0 > option.align.indexOf("left")})
        // block - text contents
        .c-text-contents
            .title.c-text-contents__headline(id=`${dialogLabelId}`)
                // 22.11.11 - moved font classname here
                strong.cmp-title.font-w-light-36.font-m-light-24(data-cmp-data-layer="")= dialogHeadline
                    
                if (option.pay)
                    .cmp-title-img
                        img(src="../../assets/image/logo-kaspi-kz.svg")
            if option.headerDescription
                .text.c-text-contents__bodycopy
                    span.cmp-text.font-w-normal-16.font-m-normal-16(data-cmp-data-layer="")= dialogDescription

mixin emi-lbl-list(data)
    dl.emi-lbl-list
        each item in data
            dt= item.label
            dd= item.cost

block content
    .button.c-cta
        +defaultButton({name:"CT0030_5", wSize: 'medium', mSize: 'medium', buttonStyle: 'highlight'})(href="#CT0030_5", class="js-pop-open", roleButton="true")
        +defaultButton({name:"CT0030_3", wSize: 'medium', mSize: 'medium', buttonStyle: 'highlight'})(href="#CT0030_3", class="js-pop-open", roleButton="true")
        +defaultButton({name:"CT0030_4", wSize: 'medium', mSize: 'medium', buttonStyle: 'highlight'})(href="#CT0030_4", class="js-pop-open", roleButton="true")
        //- 2023-05-26 : UPDATE -KlarnaPopup
        +defaultButton({name:"AT_Popup", wSize: 'medium', mSize: 'medium', buttonStyle: 'highlight'})(href="#AT_KlarnaPopup", class="js-pop-open")
