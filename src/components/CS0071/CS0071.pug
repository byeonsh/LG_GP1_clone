                                    span.c-step-progress__number.font-m-normal-16.font-w-normal-16 1
                                    p.c-step-progress__text.font-m-normal-14.font-w-normal-12 Receipt
                                li.c-step-progress__item
                                    span.c-step-progress__number.font-m-normal-16.font-w-normal-16 2
                                    p.c-step-progress__text.font-m-normal-14.font-w-normal-12 Progress
                                li.c-step-progress__item
                                    span.c-step-progress__number.font-m-normal-16.font-w-normal-16 3
                                    p.c-step-progress__text.font-m-normal-14.font-w-normal-12 Completed
                            button(type="button").c-step-progress__cancel
                                span.c-close__icon
                                p.font-m-normal-14.font-w-normal-12 Cancel

                        +printLazyPicture('https://via.placeholder.com/230x230/ccc/eee/?text=230x230', 'https://via.placeholder.com/300x300/ccc/eee/?text=300x300')
