extends ../_common/_layout.pug
include ../_common/_button.pug
include ../_common/_headline.pug
include ../_common/_image.pug

block meta 
    - var componentId = "CS0071"
    - var componentName = "Track My Repair_Result"
    - var description = "description text"
    - var keywords = "keyword list"
    title #{componentId}. #{componentName}
    meta(name="description" content=description)
    meta(name="keywords" content=keywords)

block styles
    link(rel="stylesheet", href=componentId+".css")

block scripts
    //- script(src=componentId+".js")
    script(src=componentId+".js" type="module")
 
block content

    mixin trackBtns
        // block : cta
        .button.c-cta
            +defaultButton({name: 'Send a Message', wSize: 'medium', mSize: 'small', buttonStyle: 'default', isButton: false})
            +defaultButton({name: 'Print Out', wSize: 'medium', mSize: 'small', buttonStyle: 'default', isButton: false})(href="#" class="c-print c-hidden-mo" role="print")
            +defaultButton({name: 'Previous', wSize: 'medium', mSize: 'small', buttonStyle: 'default', isButton: false})

    .guide-text-type Track My Repair_Result
    .c-wrapper.CS0071.type-text.margin-top.margin-bottom
        .component.c-repair__review
            .CS0071__container
                .c-cs-header
                    // block : text-contents
                    .c-text-contents
                        .title.c-text-contents__headline 
                            +headline("font-w-light-56 font-m-light-28", h2, "Track my repair")
                        .text.c-text-contents__bodycopy
                            .cmp-text.font-w-normal-20.font-m-normal-12(data-cmp-data-layer="")
                                span Review your requested repair service.
                    .button__group
                        .button.c-cta
                            +defaultButton({name: 'Go to support hub', wSize: 'large', mSize: 'medium', buttonStyle: 'default'})
                            +defaultButton({name: 'Inquiry further', wSize: 'large', mSize: 'medium', buttonStyle: 'highlight', isButton: "none"})

                .c-regist__complete-wrap
                    .c-prd-info
                        .c-step-progress 
                            ol.c-step-progress__list
